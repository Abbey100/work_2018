@charset "utf-8"; 

/*sorting*/
.sorts {position:relative; padding:13px 0 15px 15px; border-top:1px solid #969696;}
.sorts ul {width:100%; overflow:hidden;}
.sorts ul li {float:left; padding:0 0 0 4px;}
.sorts ul li a {color:#969696;} 
.sorts ul li a:hover {color:#969696; }
.sorts ul li.selected a {color:#3858ed; }
.sorts ul li.selected a:hover {color:#3858ed; }
.sorts ul li:before {color:#afafaf; content:'ㆍ'; padding-right:1px;}
.sorts ul li:first-child:before {content:'';}
.vertical_lf .sorts {border-top: 0 none; border-bottom: 1px solid #ededed;}

/* 신청게시판 샹태 */
.state {display:inline-block; min-width:44px; height:22px; font-size:12px; color:#fff; text-align:center; line-height:21px; border-radius:2px; min-width:44px; }/*padding:0 11px; 패딩이 없는 경우가 있어 삭제 by 백은경 */
.state.waiting {background:#9499b0;} /*대기*/
.state.companion {background:#666;}
.state.acknowledgment {background:#5a79ec;}
.state.agree {background:#eeeeee; color:#666;}
.state.cancel {background:#fa8e4b; letter-spacing:-2px;}
.state.ongoing {background:#5a79ec; } /*진행중*/
.state.end {background:#eeeeee; color: #666; } /*종료*/

.state.standby {padding-right:9px;font-size:13px; color:#333;text-align:right}

/* Table */
td.location, td.capacity { color:#969696; }

/* 파일 확장자 아이콘 */ /*2018-04-16 뮤직아이콘 추가. by 황정화*/
.ico_file {display:inline-block; width:16px; height:16px; background: url(../img/ico_file.png) 0 0 no-repeat; font-size: 0; vertical-align: top;}
.ico_file.doc{background-position: 0 0}
.ico_file.xls{background-position: 0 -22px}
.ico_file.ppt{background-position: 0 -44px}
.ico_file.zip{background-position: 0 -66px}
.ico_file.pdf{background-position: 0 -88px}
.ico_file.folder{background-position: 0 -109px}
.ico_file.shar{background-position: 0 -129px}
.ico_file.txt{background-position: 0 -150px}
.ico_file.hwp{background-position: 0 -172px}
.ico_file.etc{background-position: 0 -194px}
.ico_file.image{background-position: 0 -216px}
.ico_file.video{background-position: 0 -238px}
.ico_file.music{background-position: 0 -260px}

/* 상위폴더로 이동 */
.parent_folder {display:inline-block; width:30px; height:14px; background: url(../img/ico_parent_folder.png) 0 0 no-repeat; font-size: 0; vertical-align: top;}

.hgroup .flag_share{display:inline-block;width:36px;height:20px;margin:5px 5px 0 0;border-radius:2px 2px;background-color:#5a79ed;text-align:center;color:#fff;font-weight:bold;}

/* 썸네일형 목록. 설문조사+동영상 */
.thumbnail_lst_head, .album_lst_head{border-top:1px solid #969696;}
.thumbnail_lst, .album_lst{}
.thumbnail_lst ul, .album_lst ul{margin-left:15px;}
.thumbnail_lst ul li{position:relative;float:left;width:215px;height:224px;margin:19px 10px 0 0;}
.thumbnail_lst ul li .check, .album_lst ul li .check{padding-bottom:9px;}
.thumbnail_lst ul li .thumb, .album_lst ul li .thumb{position:relative;margin-bottom:10px;}
.thumbnail_lst ul li .thumb a, .album_lst ul li .thumb a{display: block;}
.thumbnail_lst ul li .thumb .ico_play:after{content:'';position:absolute;top:42px;left:89px;width:38px;height:38px;background: url(../img/ico_play.png) 0 0 no-repeat;z-index: 99}
.thumbnail_lst ul li a.subject, .album_lst ul li a.subject{display:block;overflow: hidden; color:#111;font-size:14px;text-overflow: ellipsis; white-space: nowrap;}
.thumbnail_lst ul li .research_info, .album_lst ul li .research_info{font-size: 12px;color:#969696;margin-top:2px;}
/*.thumbnail_lst ul li .research_info .name:after, .album_lst ul li .research_info .name:after{content:'';display: inline-block;width:1px;height:12px;margin:0 3px 0 6px;background-color: #d9d9d9;vertical-align: middle;}*/
.thumbnail_lst ul li .research_info .participation, .thumbnail_lst ul li .research_info .date{display: inline-block;vertical-align:-1px;}
.thumbnail_lst ul li .research_info .d_day, .thumbnail_lst ul li .research_info .research_date, .thumbnail_lst ul li .research_info .count{position:absolute;top:120px;right:0;}
.thumbnail_lst ul li .research_info .d_day:before{content:'';display:block;width:50px;height:28px;background-color: #000;opacity: 0.3;}
.thumbnail_lst ul li .research_info .d_day span{position:absolute;top:5px;right:6px;color:#fff;}
.thumbnail_lst ul li .research_info .research_date:before{content:'';display:block;width:215px;height:28px;background-color: #000;opacity: 0.3;}
.thumbnail_lst ul li .research_info .research_date span{position:absolute;top:5px;left:0px;width:100%;color:#fff;text-align:center;}
.thumbnail_lst ul li .research_info .count:before{content:'';display:block;width:28px;height:28px;background-color: #000;opacity: 0.3;}
.thumbnail_lst ul li .research_info .count span{position:absolute;top:4px;right:6px;color:#fff;font-size:13px;}

/* 앨범형 목록 */
.album_lst ul li{position:relative;float:left;width:170px;height:261px;margin:19px 10px 0 0;}
.album_lst ul li .research_info .name{display: inline-block; color: #111; max-width: 85px; text-overflow: ellipsis; overflow: hidden; white-space: nowrap; }
.album_lst ul li .research_info .date{position: absolute; top: 0; display: inline-block;}
.album_lst ul li .research_info .count{position:absolute;top:-62px;right:0;}
.album_lst ul li .research_info .count:before{content:'';display:block;width:28px;height:28px;background-color: #000;opacity: 0.3;}
.album_lst ul li .research_info .count span{position:absolute;top:4px;right:6px;color:#fff;font-size:13px;}
.album_lst .thumb .badge {position:absolute; top:10px; left:10px;}

/* 환경설정 hgroup과 content_table 간격 조정*/
.hgroup_btn {display:inline-block; height:28px;margin-right:8px;}
.hgroup_btn .ico_btn {float:left; margin:0 0 0 -1px !important; border-radius: 0;}
.hgroup_btn .ico_btn:first-child {border-radius: 2px 0 0 2px; margin-left: 0;}
.hgroup_btn .ico_btn:last-child {border-radius: 0 2px 2px 0;}

.hgroup_btn .btn {position:relative; float:left; margin:0 0 0 -1px; border-radius: 0;}
.hgroup_btn .btn:first-child {border-radius: 2px 0 0 2px; margin-left: 0;}
.hgroup_btn .btn:last-child {border-radius: 0 2px 2px 0;}
.hgroup_btn .btn.on {color:#3858ed; background: #ebf1fc; border:1px solid #3858ed; z-index:2;}

/* 달력 (월간) */
.monthly_list {position:relative; display: block; height: 100%;}
.monthly_list .table_head {border:none; background: #f3f6fb;}
.monthly_list .table_head th {position: static; border-top:1px solid #969696; border-bottom:1px solid #eee; height:29px; min-height:29px; padding:0 10px; font-size:12px;}

.container_area.square .monthly_list .table_body {height: 100%}
.monthly_list .table_body .wrap-hs { height: 100%; min-height: 466px !important;}
.monthly_list .table_body table { height: auto; min-height: 100% !important;}
.monthly_list .table_body td {overflow:visible; height: 25%; padding:0 !important; font-size:12px; border-right:1px solid #f5f5f5; border-bottom:1px solid #f5f5f5; vertical-align: top;}/*  height: 162px; 크기가 조절되게 해달라는 요청에 의해 높이값 변경 2017-11-17 백은경*/
.monthly_list .table_body.fourth td {height: 25%;}/* 4 주차 */
.monthly_list .table_body.fifth td {height: 20%;}/* 5 주차 */
.monthly_list .table_body.sixth td {height: 16.66%;}/* 6 주차 */

.monthly_list .table_body td:last-child {border-right:0;}
.monthly_list .table_body tr:hover th,
.monthly_list .table_body tr:hover td {background:#fff !important;}
.monthly_list td.saturday .day_date {color:#5a79ec;}
.monthly_list td.sunday .day_date,
.monthly_list td.sunday.holiday .day_date,
.monthly_list td.saturday.holiday .day_date,
.monthly_list td.holiday .day_date,
.monthly_list td.holiday .day_txt2 {color:#ff5959; max-width:calc(100% - 30px); white-space:nowrap; overflow:hidden; text-overflow: ellipsis;}
td.sunday.holiday .day_hidden .day_date,
td.saturday.holiday .day_hidden .day_date {color:#a6a6a6 !important;}

.month_cell {position:relative; height:100%; min-height: 92px !important; padding:29px 2px 0 2px; box-sizing:border-box;}
.table_body.fourth .month_cell {height: calc(25vh - 39px);} 
.table_body.fifth .month_cell {height: calc(20vh - 31px);} 
.table_body.sixth .month_cell {height: calc(16.66vh - 26px);}

.sq_sched .table_body.fourth .month_cell {height: calc(25vh - 49px);} 
.sq_sched .table_body.fifth .month_cell {height: calc(20vh - 39px);} 
.sq_sched .table_body.sixth .month_cell {height: calc(16.66vh - 32px);}


.month_cell .day_date {position: absolute; top:3px; left:3px; display:block; width: 25px; height:25px; line-height:25px; text-align: center; color:#333; border-radius: 50%; z-index:5;}
.month_cell .day_txt1 {position: absolute; top:6px; right:10px; display:block; margin:0 0 0 30px; text-align: right; color:#a6a6a6; z-index:5;}
.month_cell .day_txt2 {position: absolute; top:6px; left:28px; display:block; z-index:5;}
.month_cell .day_schedule_add {position: absolute; top:2px; left:2px; display:block; width: calc(100% - 4px); height: calc(100% - 4px); min-height:calc(100% - 4px); text-indent: -9999px; z-index:2;}
.month_cell .day_schedule_add:hover {background: #fafafa; border: 1px solid #eee;}
.month_cell .day_schedule_add:active, .month_cell .day_schedule_add.active {background: #edf3ff; border: 1px solid #dee8fc;}
.month_cell .day_schedule_more {position: absolute; left:10px; bottom:10px; display:inline-block; color:#2d3f93; text-decoration: underline; font-weight: bold; z-index:5;}
.month_cell.day_hidden .day_date {color:#a6a6a6 !important;}
.month_cell.day_hidden .day_txt2 {color:#a6a6a6 !important;}
.month_cell.today .day_date {color:#fff; background: #4c4c4c;}

.weekely_list .table_body tr.bline_no td,
.daily_list .table_body tr.bline_no td {border-bottom:none;}

