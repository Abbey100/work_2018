        <div class="container_area">
            <!-- 본문 영역 -->
			<div class="scroll_area">
            <div class="group_menu contact_tree">
                <!-- 조직도 트리 영역 -->
                <div class="org_group">
                    <ul class="tab">
                        <li><a href="" class="selected">조직도</a></li>
                        <li><a href="" >부서</a></li>
                        <li><a href="" >전사</a></li>
                    </ul>
                    <legend>조직도 검색</legend>
                    <fieldset>
                        <div class="srch_group on">
                            <!-- // 검색 이후 되돌아가는 버튼을 위해 on 추가 -->
                            <input type="text" placeholder="이름, 직위 검색" />
                            <a class="btn_srch">검색</a>
                        </div>
                    </fieldset>
                    <div class="org_tree handy-scroll">
                        <ul class="dynatree-container ">
                            <li class="">
