		<!-- Common UI Guide -->  
		<script type="text/javascript" src="../../js/common.js" ></script>	
	</head>
    <!-- popup 기본 사이즈는 280 x 680 -->
    <body>
        <!-- header -->
        <h1>공유자 선택</h1>
        <!-- // header -->
