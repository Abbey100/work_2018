		<!-- Common UI Guide -->  
		<script type="text/javascript" src="../../../js/common.js" ></script>		
		<link rel="stylesheet" type="text/css" href="../../../css/sub.css" />
	</head>
	<body>
		<!-- wrapper -->
		<div class="wrapper">    
			<!-- header -->  
			<div class="header">
				<h1>개인정보설정</h1>
				<div class="aside">
					<fieldset>
						<div class="selectbox">
							<select>
								<option selected>전체</option>
								<option>통합</option>
