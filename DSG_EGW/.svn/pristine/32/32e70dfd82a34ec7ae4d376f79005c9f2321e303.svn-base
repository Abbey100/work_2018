                <!-- content -->
                <div class="container_area handy-scroll" >
                    <div class="">
                        <!-- 본문 영역 -->
                        <div class="container">
                            <!-- 타이틀 영역 -->
                            <div class="hgroup border">
                                <h2>설정</h2>	
                                <ul class="tab">
                                    <li><a class="selected">일반설정</a></li>
                                    <li><a class="">용량 정보</a></li>	
                                </ul>
                            </div>
                            <!-- // 타이틀 영역 -->
                            <!-- form -->
                            <div class="form_table">
                                <table>
                                    <colgroup>
                                        <col style="width:170px;">
                                        <col width="*">
                                    </colgroup>
                                    <tbody>
                                        <tr>
                                            <th>목록 유형</th>
                                            <td>
                                                <div class="option">
                                                    <span class="form">
                                                        <input type="radio" name="type" id="type_recieve" checked />
                                                        <label for="type_recieve">앨범형</label></span>
                                                    <div class="group">
                                                        <span class="form">
                                                            <input type="radio" name="type" id="type_new" />
                                                            <label for="type_new">리스트형</label>
                                                        </span>
                                                    </div>
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <th>목록 개수</th>
                                            <td>
                                                <div class="option">
                                                    <span class="form">
                                                        <input type="radio" name="list_set" id="" checked />
                                                        <label for="">15개</label></span>
                                                    <span class="form">
                                                        <input type="radio" name="list_set" id="" />
                                                        <label for="">20개</label></span>
                                                    <span class="form">
                                                        <input type="radio" name="list_set" id="" />
                                                        <label for="">30개</label></span>
                                                    <span class="form">
                                                        <input type="radio" name="list_set" id="" />
                                                        <label for="">40개</label></span>
                                                    <span class="form">
                                                        <input type="radio" name="list_set" id="" />
                                                        <label for="">50개</label></span>
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <th>폴더 설명</th>
                                            <td>
                                                <div class="option">
                                                    <span class="form">
                                                        <input type="radio" name="folder" id="type_recieve" checked />
                                                        <label for="type_recieve">사용함</label></span>
                                                    <div class="group">
                                                        <span class="form">
                                                            <input type="radio" name="folder" id="type_new" />
                                                            <label for="type_new">시용 안함</label>
                                                        </span>
                                                    </div>
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <th>대용량 첨부 폴더 설정</th>
                                            <td>
                                                <div class="input_group has_btn">
                                                    <input type="text" class="basic_input" value="대용량 첨부 폴더" readonly="readonly">
                                                    <a class="ico group" href="#">조직도</a>
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <th>다운로드 가능 기간</th>
                                            <td>
                                                <div class="option">
                                                    <span class="form">
                                                        <input type="radio" name="period" id="" checked />
                                                        <label for="">10일</label></span>
                                                    <span class="form">
                                                        <input type="radio" name="period" id="" />
                                                        <label for="">20일</label></span>
                                                    <span class="form">
                                                        <input type="radio" name="period" id="" />
                                                        <label for="">30일</label></span>
                                                    <span class="form">
                                                        <input type="radio" name="period" id="" />
                                                        <label for="">무제한</label></span>
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <th>다운로드 가능 횟수</th>
                                            <td>
                                                <div class="option">
                                                    <span class="form">
                                                        <input type="radio" name="set_number" id="" checked />
                                                        <label for="">5회</label></span>
                                                    <span class="form">
                                                        <input type="radio" name="set_number" id="" />
                                                        <label for="">10회</label></span>
                                                    <span class="form">
                                                        <input type="radio" name="set_number" id="" />
                                                        <label for="">20회</label></span>
                                                    <span class="form">
                                                        <input type="radio" name="set_number" id="" />
                                                        <label for="">30회</label></span>
                                                    <span class="form">
                                                        <input type="radio" name="set_number" id="" />
                                                        <label for="">무제한</label></span>
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <th>대용량 첨부 자동 삭제</th>
                                            <td>
                                                <div class="selectbox" style="width:300px;">
                                                    <select title="반복 빈도 선택"> 
                                                        <option selected>자동삭제 안함</option>
                                                        <option>다운로드 기간 또는 횟수 초과시 휴지통 이동</option>
                                                        <option>다운로드 기간 초과시 휴지통 이동</option>
                                                        <option>다운로드 횟수 초과시 휴지통 이동</option>
                                                    </select>
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <th>휴지통 자동 삭제 기간</th>
                                            <td>
                                                <div class="option">
                                                    <span class="form">
                                                        <input type="radio" name="del_time" id="" checked />
                                                        <label for="">1주일 후</label></span>
                                                    <span class="form">
                                                        <input type="radio" name="del_time" id="" />
                                                        <label for="">1달 후</label></span>
                                                    <span class="form">
                                                        <input type="radio" name="del_time" id="" />
                                                        <label for="">자동 삭제 안함</label></span>
                                                </div>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                                <div class="btns_area">
                                    <a class="action_btn" href="#">저장</a>
                                    <a class="btn">취소</a>
                                </div>
