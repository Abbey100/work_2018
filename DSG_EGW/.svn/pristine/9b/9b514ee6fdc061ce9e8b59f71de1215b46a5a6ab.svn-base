/*layer_pop 본문영역 리스트 */
.square_layer_pop .contents {position: relative; padding:20px;}
.square_layer_pop .table_head {position:relative; border-top: 1px solid #c9c8cd; border-bottom: 1px solid #c9c8cd; width:360px; margin-top: 21px; height:32px;}
