/* 퀵버튼과 관계된 layer pop - 임시저장 */
.square_layer_pop {position:absolute; top:63px; z-index:9999999; display:none;}
.square_layer_pop.on {display:block;}
.square_layer_pop.two.first { left:90px; }
.square_layer_pop.two.second { left:167px; }
.square_layer_pop.three.first { left:66px; }
.square_layer_pop.three.second { left:128px; }
.square_layer_pop.three.third { left:190px; }
/* 기본값은 display:none 으로 처리(개발처리시 적용) */
.square_layer_pop .layer_arr { position:absolute; left:0; top:14px; display:block; background: url(../img/square/pop_arrow.png) 0 0 no-repeat; width:13px; height:12px; z-index:9999999;}
.square_layer_pop .inner_box { position:absolute; left:12px; border:1px solid #666; background-color:#fff; width:400px; height: 520px; z-index:999999; }
.square_layer_pop .inner_box .tit { height:25px; padding:14px 15px 0; border-bottom:2px solid #7171dc; font-weight: bold; }

/*layer_pop 타이틀영역 버튼 */
.square_layer_pop .inner_box .tit span.count:before {content:'('; margin-left:4px; }
.square_layer_pop .inner_box .tit span.count:after {content:')'; }
.square_layer_pop .inner_box .tit a {position:absolute; top:8px; width:24px; height:24px; cursor: pointer; font-size: 0; }
.square_layer_pop .inner_box .tit  .more { position:absolute; right:30px; background:url(../img/square/layer_more.png) 0 0 no-repeat;}
.square_layer_pop .tit .more:hover,
.square_layer_pop.modify .tit .more {  background:url(../img/square/layer_more.png) 0 -24px no-repeat;}

.square_layer_pop .tit .close { position:absolute; right:6px;  background:url(../img/square/layer_close.png) 0 0 no-repeat;}
.square_layer_pop .tit .layer_pop {position: absolute; top:34px; left:268px; width:100px; height:; font-weight: normal; display: none; z-index: 9999999;}
.square_layer_pop .tit.on .layer_pop {display: block;}


/*layer_pop 본문영역 검색 */
.square_layer_pop .layer_search
{position:relative; width:100%; display:block; height:31px; border:1px solid #d1d1d1; width:100%; height:100%; border-radius: 4px;  padding: 4px; box-sizing: border-box; }
.square_layer_pop.modify .layer_search {display: none;}
    .square_layer_pop .layer_search input {height:23px; width:100%; padding:0 5px; box-sizing: border-box; box-shadow: none; border:0 none;}
    .square_layer_pop .layer_search .btn_search {position:absolute; display:block; top: 8px; right:8px; background:#fff url(../img/btn_srch_prpl.png) 0 0 no-repeat; width:15px; height:15px; font-size: 0;}

/*layer_pop 본문영역 수정 */
.square_layer_pop .modi_btn_area {position:relative; display: none; height:32px; }
.square_layer_pop.modify .modi_btn_area {display: block;}
.modi_btn_area .guide {position:relative;display: inline-block; width:225px; line-height: 1.5}
