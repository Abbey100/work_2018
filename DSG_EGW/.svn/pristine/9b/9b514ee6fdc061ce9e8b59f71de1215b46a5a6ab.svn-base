.work_state { display:block; color:#666; font-size: 12px;height: 20px; line-height: 20px;min-width:100px; overflow: hidden; white-space: nowrap; text-overflow: ellipsis; margin-top: 14px;}
.work_state .ing {padding:3px 4px 2px 3px;background: #65b73c;color:#fff; border-radius: 2px; font-size: 11px;height: 15px; line-height: 18px;}
.work_state .delay {padding:3px 4px 2px 3px;background: #ce5656;color:#fff; border-radius: 2px; font-size: 11px;height: 15px; line-height: 18px;}
.work_state .fin {padding:3px 4px 2px 3px;background: #777777;color:#fff; border-radius: 2px; font-size: 11px;height: 15px; line-height: 18px;}
.work_state .classify:after {content:' :'; margin-right:5px;}

/* 검색된 지시업무 카드 hover */
