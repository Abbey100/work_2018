    <!-- popup 기본 사이즈는 800 x 680 -->
    <body>
        <!-- header -->
        <h1>배부처 지정</h1>
        <!-- // header -->
        <!-- body -->
        <div class="container_area">
            <!-- 본문 영역 -->
            <div class="scroll_area">
            <div class="handy-scroll">
                <div class="appr_flow_wrap">
                    <div class="inner_wrap">
                        <ul>
                            <li class="on"><p class="appr_flow">부서 지정</p></li>
                            <li><p class="appr_flow">의견 작성</p></li>
                        </ul>
                    </div>
                </div>
                <div class="group_menu sche_share_menu">
                    <!-- 조직도 트리 영역 -->
