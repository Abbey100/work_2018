<script type="text/javaScript">

function _setCookie(name, value) {
    if (value == null) {
        return;
    }
    
    exp = new Date();
    exp.setMonth(exp.getMonth() + 1);
    value = escape(value);
    document.cookie = name + "=" + value + ";" + ";path=/;expires=" + exp.toGMTString() + ";";
}

function showInfo(jspName, tabindex) {
    if (jspName == "") {
        jspName = '/bms/cz/cb/viw/retrieveReportCours.act';
    }
    
    jspName += "?DOCID=JHOMS151070001701000";
    jspName += "&APPLID=2020";
    jspName += "&APPLTYPE=2";
    jspName += "&DEPTID=000024006";
    jspName += "&SENDID=";
    jspName += "&PARSENDID=";
    jspName += "&REPLYID=";
    jspName += "&PARREPLYID=";
    jspName += "&USERID=001013122";
    jspName += "&APPRTYPE=7";
    jspName += "&DOCATTR=1000000000000000000000000000000000000000000000000000";
    jspName += "&ORGDOCID=";
    jspName += "&FLDRSHARE=false";
    jspName += "&WORDTYPE=7";
    jspName += "&RECDOCSTATUS=";
    jspName += "&detachedDocNo=0";
    jspName += "&regClass=";
    jspName += "&VIEWRANGE=0";
    jspName += "&ASSIGNUSERID=";
    jspName += "&CURDEPTID=000024006";
    jspName += "&BROADUSERID=";
    jspName += "&EXAMID=";
    
    var url = jspName;

    _setCookie("showTab",tabindex);

    D$("docInfoFrame").src = url;
}

function winClose() {
    if (parent.doToggleInfoWnd) {
        parent.doToggleInfoWnd(false);
    }
}

