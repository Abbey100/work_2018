.gnb_area{position:fixed;top:0;width:58px;height:100%;background-color:#29292c;border-bottom: 1px solid #bfbfbf;z-index:999;}
.gnb{}
.gnb_menu.first{position:relative;width:58px;height:58px;background-position:50% 50%;background-repeat:no-repeat;}
.gnb_menu.first:hover{background-color:#1b1b1c;cursor:pointer;}
    .gnb_menu.first.home{background-image:url('../img/gnb_ico_home.png');}
    .gnb_menu.first.menu{background-image:url('../img/gnb_ico_menu.png');}
    .gnb_menu.first.set{background-image:url('../img/gnb_ico_set.png');}
    .gnb_menu.first.statistics{background-image:url('../img/gnb_ico_statistics.png');}
    .gnb_menu.first.system{background-image:url('../img/gnb_ico_home.png');}
.gnb_menu.second {position:absolute;left:58px;display:none;width:185px;padding:20px;background-color:#1b1b1d;}
.gnb_menu.second > ul {padding-left: 10px;}
.gnb_menu.first:hover .gnb_menu.second {display:block;} 
.gnb_menu h2{width:100%;height:26px;padding:0;color:#fff;font-size:15px;letter-spacing: -0.7px}
.gnb_menu .no_child {position:relative;display:block;width:100%;padding:10px 0 6px;font-size:14px;color:#fff}
.drop_down{}
.gnb_menu .drop_down {position:relative;}
.gnb_menu .second_ttl {position:relative;display:block;width:100%;padding:10px 0 6px;font-size:14px;color:#eaeaea}
.gnb_menu .drop_down .second_ttl:after{content:'';position:absolute;top:0;right:0;display: inline-block;width:6px;height:10px;margin:13px 0 0 15px;background: url('../img/sp_common.png') no-repeat -10px -50px;}
.gnb_menu .drop_down.open .second_ttl:after{content:'';position:absolute;top:0;right:0;display: inline-block;width:10px;height:6px;margin:13px 0 0 15px;background: url('../img/sp_common.png') no-repeat -164px -26px;}
.gnb_menu.third{display:none;}
.drop_down.open .gnb_menu.third{display:block;}
.gnb_menu.third li{position: relative;height:25px;}
.gnb_menu.third li a{line-height:25px;padding-left:8px;padding-right:30px;color:#bbbbbd;font-size:12px;letter-spacing: -0.08em;}
.gnb_menu.third li a:hover, .gnb_menu.third li a.current{color:#0ad7c8}
.gnb_menu.third li a:before{content:'';position:absolute;top:11px;left:0;width:2px;height:2px;background: url('../img/sp_common.png') no-repeat -88px -26px;}
.gnb_menu.third li a:hover:before, .gnb_menu.third li a.current:before{content:'';position:absolute;top:11px;left:0;width:2px;height:2px;background: url('../img/sp_common.png') no-repeat -92px -26px;}

