/* 컨텐츠 영역 */
.body{position:relative;top:58px;left:280px;width:calc(100% - 280px);height:calc(100% - 58px);padding:0;margin:0;overflow-x: auto; overflow-y: hidden}
.body .container_area{position:relative;width:100%;min-width:1000px;height:100%;padding:30px 30px 0;box-sizing: border-box;}/* 2017-09-20 컨텐츠 넓이값 수정. box-sizing 속성때문에 좌우 패딩값 추가된 넓이값. by 정수진 */
.body .left_area{position:absolute;width:190px;height:100%;margin-right:20px;padding:10px 10px;border:1px solid #e3e3e3;box-sizing:border-box;z-index:998;}/*2017-08-22 문서의 height를 체크하기 위해 height:100% 삭제. min-height:100% 추가 */
.body .container_area.tree_out{background: none}
.body .container_area.tree_out:before{content:'';display: none}
.body .title_area{position:relative;min-width:900px;height:35px;padding: 0 0 5px;background-color:#fff;}
.title_area span.title{display:inline-block;max-width:100%;line-height:24px;color:#202020;font-size:20px;letter-spacing:-0.7px;white-space:nowrap;overflow:hidden;text-overflow: ellipsis;word-break:break-all;}
.title_area .location{position:absolute;top:50px;right:40px;color:#666;font-size:12px}
.title_area .location span{padding:0 4px;font-weight: 400}
.title_area .location:before{content:'';display: inline-block;width:12px;height:10px;margin-right:7px;background: url('../img/sp_common.png') no-repeat -132px -26px;vertical-align:-1px}

/* lnb */
.body .lnb_area {position:fixed;top:58px;left:58px;display:block;width:222px;height:calc(100% - 58px);background:#f7f7f7;z-index:998;}
.lnb{padding:24px 0 0 0}
.lnb_menu.first{position:relative;padding:4px 0 0 46px;font-weight: bold;color:#202020;}
.lnb_menu.first .first_ttl{height:27px; line-height: 27px;}
.lnb_menu.first.drop_down .first_ttl:before{content:'';position:absolute;top:14px;left:30px;width:11px;height:6px;background: url('../img/sp_common.png') no-repeat -195px -50px;}
.lnb_menu.second{position:relative;display: none;padding-left:10px;}
.drop_down.open .lnb_menu.second{display: none;}
.drop_down.open.open .lnb_menu.second{display: block;}
.lnb_menu.second li {position:relative;height:21px; line-height: 21px;}
.lnb_menu.second li a{color:#666;padding:0 0 0 7px;}
.lnb_menu.second li a:hover, .lnb_menu.second li.current a, .lnb_menu.first .first_ttl.current{color:#3858ed}
.lnb_menu.second li a:before{content:'';position:absolute;top:9px;left:0;width:2px;height:2px;background: url('../img/sp_common.png') no-repeat -88px -26px;}
.lnb_menu.second li a:hover:before, .lnb_menu.second li.current a:before{content:'';position:absolute;top:9px;left:0;width:2px;height:2px;background: url('../img/sp_common.png') no-repeat -92px -26px;}

.body .content_section{height:calc(100% - 40px)}
.body .content_area{position:relative;height:100%;padding:0 0 50px 215px;box-sizing: border-box;}
.body .content_section.tree_out .content_area{padding-left:0}



/* 서브 탭 */
.tab_area{overflow: hidden;text-align:center;}
.tab_area ul {position:relative; display: block; width:100%; height:41px;border-bottom:1px solid #202020;}
.tab_area ul li{width:200px;height:41px;float:left;border-left:1px solid #fff;box-sizing:border-box;}
.tab_area ul li:first-child{border-left:0 none;}
.tab_area ul li a{display:block;height:41px;padding:0 10px;border-width:1px 1px 0 1px;border-style:solid;border-color:#ddd;border-radius: 2px 2px 0 0;background-color:#f7f7f7;color:#999;box-sizing:border-box;}
.tab_area ul li a .tab_title{display:inline-block;max-width:82%;height:42px;line-height:42px;font-weight:bold;vertical-align:middle;font-size:15px;letter-spacing: -0.7px;white-space:nowrap;overflow:hidden;text-overflow: ellipsis;word-break:break-all;}
.tab_area ul li a:hover{background-color:#474747;border:none;color:#fff;line-height:41px;}
.tab_area ul li a:active{background-color:#000;color:#fff}
.tab_area ul li.current a{position:absolute; width:199px; bottom:-1px; height:42px;line-height:39px; border-color:#202020; border-bottom:1px solid #fff;z-index: 999;background-color:#fff;color:#202020;}
.tab_area ul li.current a:hover{border:1px solid #202020;border-bottom:none;}
.tab_area ul li:first-child.current a{width:200px;}
.tab_area ul li.current {position:relative;}
.tab_area ul li span.list_num{display:inline-block;width:18px;height:18px;line-height:18px;margin-left:4px;border-radius:18px 18px;background-color:#ee3425;color:#fff;font-size:12px;text-align:center;vertical-align:middle;}
.tab_area.tab_multi ul li{width:139px;}
.tab_area.tab_multi ul li a .tab_title{max-width:73%;}
.tab_area.tab_multi ul li.current a{position:absolute; width:138px;}
.tab_area.tab_multi ul li:first-child.current a{width:139px;}
.tab_area.tab_multi2 ul li{width:104px;}/* 2017-09-19 서브 탭 넓이값 수정. by 정수진 */
.tab_area.tab_multi2 ul li a .tab_title{max-width:64%;}
.tab_area.tab_multi2 ul li.current a{position:absolute; width:103px;}
.tab_area.tab_multi2 ul li:first-child.current a{width:104px;}
.tab_area.tab_child{padding:7px 5px 0 5px;border-bottom:0;}
.tab_area.tab_child ul li {width:134px;border:0;padding: 0 5px;}
.tab_area.tab_child a {background: #eeeeee;}
.tab_area.tab_child ul li a .tab_title {font-size: 14px;line-height:26px;font-weight: bold;letter-spacing:-1px;color: #999999}
.tab_area.tab_child ul li a.current {background: #fff;}
.tab_area.tab_child ul li a.current .tab_title {color: #333333;text-decoration: underline;}
