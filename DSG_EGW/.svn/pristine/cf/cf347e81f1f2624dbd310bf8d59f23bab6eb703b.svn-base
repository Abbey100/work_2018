/* 체크박스, 라디오버튼 custom-checkbox, custom-radio 의 명칭 checkbox, radio로 변경 */
.checkbox, .radio {position:relative;display:inline-block;min-width:15px;vertical-align: middle;}
.checkbox input, .radio input {position: absolute;top: 0;left: 0;margin:3px 0 0 1px;z-index: 0;}
.checkbox label, .radio label {position: relative;display:inline-block;padding-right:5px;cursor: pointer;z-index: 1;}
.checkbox label:before, .radio label:before{content:'';display:inline-block;width:20px;}
.checkbox label { background: url(../img/ico_checkbox.png) no-repeat;}
.radio label { background: url(../img/ico_radiobutton.png) no-repeat;}
.checkbox label, .radio label {background-position: 0 3px;}
.checkbox label img, .radio label img{margin:0 4px 0 3px;vertical-align:-4px;}
 .checkbox label.checked, .radio label.checked {background-position: 0px -97px;}
.checkbox label.disabled, .radio label.disabled{background-position: 0px -197px;border:none;background-color: transparent; cursor: default;}


