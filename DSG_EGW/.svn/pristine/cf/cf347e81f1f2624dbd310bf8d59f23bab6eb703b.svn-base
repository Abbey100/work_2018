@charset "UTF-8";

html{height:100%;}
body {
	position:relative;
    width: 100%;
    height:100%;
	padding:0;
	/*background:#fff url('../img/wapper_back.gif') repeat-y 0 0;*/
	color: #666;
    font-size: 12px;
	font-family: "맑은 고딕","Malgun Gothic",dotum,AppleGothic,Helvetica,tahoma,sans-serif;
    text-rendering: optimizeLegibility;
    font-smoothing: antialiased;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
	line-height: 18px;
}

a, address, article, aside, body, caption, dd, dl, div, dt, em, fieldset, figcaption, footer, form, h1, h2, h3, h4, h5, h6, html, iframe, img, label, legend, li, nav, ol, p, section, span, strong, table, tbody, tfoot, thead, th, tr, ul, input, button, select, textarea {margin: 0;padding: 0;/*font-family: "맑은 고딕","Malgun Gothic",dotum,AppleGothic,Helvetica,tahoma,sans-serif;*/}
em, address { font-style: normal; }
img, input, button, select, textarea { vertical-align: middle;}
ul, ol { list-style: none outside none;}
img, fieldset { border: 0 none; }
table {width: 100%;border-spacing: 0;border-collapse: collapse;}
caption, th, td {margin: 0;padding: 0;vertical-align: middle;text-align: left; font-weight: normal;}
legend, hr, caption {position: absolute;top: 0;left: 0;width: 0;height: 0;line-height: 0;visibility: hidden;overflow: hidden;font-size: 0;}
a {cursor:pointer; outline: none; selector-dummy: expression(this.hideFocus=true);}
a:link, a:visited {color: #999;text-decoration: none;}
a:active, a:hover {color: #3858ed;text-decoration: none;}
label {	display: inline; cursor: pointer;}
em { font-style: normal;}
input {padding:0;vertical-align: middle;outline:none;background-color:#fff;border:0 none;color:#666;font-size: 12px;font-family: "맑은 고딕","Malgun Gothic",dotum,AppleGothic,Helvetica,tahoma,sans-serif;box-sizing:border-box;}
input.radio {border: none;vertical-align: middle;padding: 0;margin: -1px 0 0 0;background-color: transparent;}
textarea {border:0 none;background-color: #fff;padding:0;line-height: 18px;overflow: auto;color: #666;font-size: 12px;font-family: "맑은 고딕","Malgun Gothic",dotum,AppleGothic,Helvetica,tahoma,sans-serif;white-space: pre-wrap; word-wrap: break-word; resize: none;outline:none; }
select { color: #666; border:0 none; padding: 0;outline:none;}

/* placeholder 속성 */
input::-webkit-input-placeholder, textarea::-webkit-input-placeholder { color:#bbb}
input::-moz-placeholder, textarea::-moz-placeholder { color:#bbb}
input::-ms-input-placeholder, textarea::-ms-input-placeholder { color:#bbb}

/* ie10에서 x를 없애는 css */
input[type="text"]::-ms-clear{ height:0; width:0;display: none !important;}
input[type="search"]::-ms-clear{ height:0; width:0;display: none !important;}
input[type=password]::-ms-reveal{ display: none !important;}
input[type="checkbox"], input[type="radio"] { margin:0 6px 0 0;}

.hidden {position: absolute; visibility: hidden; width: 0;height: 0; margin: 0;padding: 0; background: none repeat scroll 0 0 transparent;font-size: 0;line-height: 0;}
.height_full{height:100%;}

/* layout css */
.wrapper{position:relative;width:100%;height:100%;}
.header{position:fixed;width:100%;height:58px;background-color:#fff;border-bottom: 1px solid #bfbfbf;z-index:999;}/*2017-08-22 layout fixed setting 으로 인해 z-index 를 최상위로 올림, 2017-09-05 layout 변경으로 인해 스타일 수정*/
.header h1.logo{background-color:#202020;height:38px;padding-top:22px}
.header h1.logo a{display:block;width:164px;height:16px;margin:0 auto;background: url('../img/sp_common.png') no-repeat -1px -1px;font-size:0;}
.header h1.area_title{position:absolute;top:20px;left:90px;font-size:19px;color:#000;width:auto;}

/* 컨테츠 스크롤 영역 */
.scroll_area{height:100%;overflow-y:auto;}/* 레이어팝업 트리영역 */
.scroll_area_tab{height:calc(100vh - 246px);padding-top:20px;overflow-y:auto;}/* 상세 페이지 하단 버튼영역 고정. 서브탭 있는 경우 */
.scroll_area_notab{height:calc(100vh - 204px);padding-top:20px;overflow-y:auto;}/* 상세 페이지 하단 버튼영역 고정. 서브탭 없는 경우 */
.scroll_area_notab02{height:calc(100vh - 148px);padding-top:20px;overflow-y:auto;}/* 상세 페이지 하단 버튼영역 없음. 서브탭 없는 경우 */
.scroll_area_srch{height:calc(100vh - 187px);overflow-y:auto;}/* 목록 상단 조회 영역 있거나 paginate이 없는 경우. 서브 탭 이하 스크롤바 생김. */
.scroll_area_graph{height:calc(100vh - 210px);padding-top:40px;overflow-y:auto;}/* 서브 탭 이하 스크롤바 생김. 시스템 디스크 현황 메뉴 사용 */
.scroll_area_no{overflow: hidden;}/* 컨텐츠 영역 스크롤바 없는 경우 */
.scroll_area_board{height:100%;overflow-y:auto;}/* 게시판 목록 스크롤. */
/*.scroll_area_board02{height:calc(100vh - 326px);overflow-y:auto;}*/ /* 게시판 목록 스크롤. 상단 조회 영역 없는 경우 */ /* 2017-09-14 목록 스크롤 영역 스크립트 공통 작업으로 삭제. by 정수진 */
.scroll_vertical_area{/*width:100%;overflow-x:auto;*/}/* 결과 테이블 필드 수 많을 때 가로 스크롤 */

/* USER MENU AREA at the right of header */
.header .user_info{padding:11px 20px 14px;}
.user_info .user_name{margin-bottom:2px;color:#ddd;font-size:12px;text-align:center;display:inline-block;width:100%;white-space: nowrap;overflow: hidden;text-overflow: ellipsis;word-break: break-all;}
.user_info .user_name:before{content:'';display: inline-block;width:16px;height:16px;margin-right:6px;background: url('../img/sp_common.png') no-repeat -1px -26px;vertical-align:middle}
.user_menu .btn_groupware{position:absolute; display:block;top:15px; right:198px;width:26px;height:26px;background:url('../img/sp_common.png') no-repeat -244px 0;font-size: 0;overflow: hidden;}
.user_menu .btn_noti{position:absolute; display:block;top:15px; right:89px;width:20px;height:23px;background:url('../img/sp_common.png') no-repeat -200px 0;font-size: 0;}
.user_menu .btn_noti.on{background:url('../img/sp_common.png') no-repeat -174px 0;}
.user_menu .btn_contextual{position:absolute; display:block;top:15px; right:45px;width:15px;height:20px;background:url('../img/sp_common.png') no-repeat -222px 0;font-size: 0;}
.user_photo {position:absolute;top:15px;right:144px;width:26px;height:26px;border-radius:50%;overflow:hidden;}
.user_photo img{width:100%; height:100%;}

/* 사용자 단 유저 메뉴 이식 - 2018-08-07 */
.header .aside {position:absolute; top:13px; right:40px;}
.header .util {float:left; padding-left:12px; font-size:0;}
.header .util a {display:inline-block; width:34px; height:34px; margin-left:12px; vertical-align: top; font-size:0; text-align:center; background:url(../img/ico_util.png) no-repeat left top;}
.header .util a.btn_admin {background-position:left top;}
.header .util a.btn_me {background:none;}
.header .util a.usr_more {width:15px; background: url('../img/ico_up_down.png') right 16px no-repeat; padding:0; margin: 0;}
.header .util a.usr_more.open {background-position:right -30px;}
.header .util a.btn_me img {width:26px; height:26px; margin-top:3px; border-radius:50%;}
.header .util a.btn_noti {position:relative; background-position:left -60px;}
.header .util a.btn_noti span {display:none;}
.header .util a.btn_noti span.on {position:absolute; right:5px; top:7px; display:inline-block; width:8px; height:8px; background:#ff5959; border-radius:50%; border:2px solid #fff;}
.header .util a.btn_support {background-position:left -120px;}
.header .tab {display: inline-block; height:21px;}
.header .ui-button:hover {background-color: transparent}


