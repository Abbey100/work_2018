.appr_prop_wrap .prop {display:inline-block; overflow: hidden; width: 16px; height: 16px; margin:0; text-indent: -9999px; background: url('../img/ico_list_appr2.png') no-repeat left top; margin-right:2px;}

/* 16 x 16 */
/* 기안한문서 */
.appr_prop_wrap .prop.draft {background-position-x: 0;}
/* 수신문서 */
.appr_prop_wrap .prop.receive {background-position-x: -16px;}
/* 비전자문서 */
.appr_prop_wrap .prop.n_elect {background-position-x: -32px;}
/* 편철문서 */
.appr_prop_wrap .prop.filing {background-position-x: -48px;}

/* 10 x 10 */
/* 공람 */
.appr_prop_wrap .prop.cc { width: 10px; height: 10px; background-position-x: -64px; margin-top:3px;}
/* 관련문서 */
.appr_prop_wrap .prop.doc {width: 10px; height: 10px; background-position-x: -74px; margin-top:3px;}
/* 열람제한 */
.appr_prop_wrap .prop.restricted {width: 10px; height: 10px; background-position-x: -84px; margin-top:3px;}
/* 이력관리 */
.appr_prop_wrap .prop.history {width: 10px; height: 10px; background-position-x: -94px; margin-top:3px;}
/* 의견 */
.appr_prop_wrap .prop.cmmt {width: 10px; height: 10px; background-position-x: -104px; margin-top:3px;}





/*.appr_view_wrap {padding:0 30px;}*/
.appr_view_head {position: relative; padding:19px 0 0 30px; background: #f9f9f9;}
.appr_view_head .title {position:relative; padding: 0 24px 0 25px;}
.appr_view_head .title .form.flag {position: absolute; left:0; top:6px;}
.appr_view_head .title .badge {margin:6px 0 0 0; vertical-align: top;}
.appr_view_head .title .subject {margin:-2px 0 0 0; font-size: 17px; color:#111; font-weight: bold; line-height:23px;}
.appr_view_head .title .ico.pop_up {display:inline-block; margin: 4px 0 0 0; vertical-align: top;}
.appr_view_head .sub_info {padding: 0 24px 0 25px; margin:10px 0 0 0;}
.appr_view_head .sub_info .date {position: relative; display:inline-block; padding:0 0 0 8px; margin:0 0 0 8px; color:#666;}
.appr_view_head .sub_info .date em {font-weight: bold;}
.appr_view_head .sub_info .date:after {content:''; position: absolute; top:3px; left:0; display:block; width:1px; height:12px; background: url('../img/divider.png') left top no-repeat; }
.appr_view_head .sub_info .date:first-child {padding:0; margin:0;}
.appr_view_head .sub_info .date:first-child:after {content:''; display:none;}
.appr_view_head .sub_info .team:before {content:'ㆍ'; color:#b3b3b3;margin-left:-3px;}
.appr_view_head .ico_btn.print {position: absolute; bottom:5px; right:15px; width:30px; margin:0; background-position: -178px -45px; background-color: #fff; z-index: 9; }
.appr_view_head .contents_tab {position: relative; height:51px; padding:23px 0 0 0 !important; border-bottom: 1px solid #ccc;}
.appr_view_head .contents_tab li a:after{display:none;}

.appr_view_detail { display: block; }
.appr_view_wrap .attached {padding:20px 30px 18px 30px; border-bottom:1px solid #eee;}
.appr_view_wrap .file_info {padding:0;}
.appr_view_wrap .file_info .btn_open.open{margin:0 0 0 -5px;}

.appr_view_cont { padding: 0 30px; }
.appr_view_cont .cont_title {position: relative; display:block; margin:23px 0 15px 0; color:#111; font-weight: bold; font-size: 13px;}
.appr_view_cont .cont_title .num {margin:0 0 0 5px; font-size: 12px; color:#666;}
.appr_view_cont .cont_title .pos_r {position: absolute; top:-2px; right:0;}
.appr_view_cont .cont_title .pos_r span {font-weight: normal}
.appr_view_cont .cont_title .pos_r .btn {margin:0;}
.appr_view_cont .cont_title .pos_r .selectbox {width:auto; display: inline-block}

.appr_view_cont .cont_title + .btns {position:relative; display:block; height: 30px; margin-bottom: 10px;}
.appr_view_cont .btns .group .btn {margin-right: 4px}
.appr_view_cont .btns .group.align_rg {position:absolute; top:0; right: 0;}
.appr_view_cont .btns .group.align_rg .txt {line-height: 26px;}
.appr_view_cont .cont_title .btn_open { width: auto; height: auto; padding-left: 13px; background-position: -5px 0; }
.appr_view_cont .cont_title .btn_open:hover { text-decoration: none; }
.appr_view_cont .cont_title .btn_open.open { background-position: -5px -21px; }
.appr_view_cont .cont_title .btn_open strong { display: inline-block; font-size: 14px; }

.appr_view_cont .replybox {margin:25px 0 0 0;}
.appr_view_cont .replybox .write {padding:0 100px 0 18px;}
.appr_view_cont .replybox .write .text_btn.fir {right:42px;}
.appr_view_cont .replybox .write .text_btn.fir:after {content:''; width:9px; height:26px; display: inline-block; background: url('../img/divider.png') 50% 4px no-repeat;padding:0 3px 0 4px; }

/*리스트에서 미리보기 모달 팝업*/
.popup_container .appr_view_head { padding: 19px 0 0 30px; }
.popup_container .appr_view_head .title { height: inherit; line-height: inherit; border-bottom: none; }
.popup_container .appr_view_wrap .attached .file_info dt:first-child { margin-top: 0; font-size: 14px; }
.popup_container .appr_view_wrap .attached .file_info dd { margin-top: 1px; }
.popup_container .appr_view_cont .vertical_table { border-top: 1px solid #969696; }
.popup_container .appr_view_cont .content_table { margin: 0; border-bottom: none; }
.popup_container .appr_view_cont .content_table .table_head th:last-child { padding: 5px 0; }
.popup_container .vertical_ui .appr_view_cont .content_table .table_head th { height: 28px; }

/* head */
.vertical_ui { position: relative; }
.vertical_ui .appr_view_cont .content_table  {border-bottom:1px solid #e3e3e3;}
.vertical_ui .appr_view_cont .content_table .table_head {min-height:30px; visibility:visible !important;}
.vertical_ui .appr_view_cont .content_table .table_head th:last-child {padding:5px 0;}
.vertical_ui .appr_view_cont .content_table .table_head th { height: 28px; }
.vertical_ui .appr_view_cont .content_table td {height:auto;}
.vertical_ui .appr_view_cont .content_table tr:last-child td {border-bottom:none;}
.vertical_ui .appr_view_cont .content_table .table_body {max-height:276px;}
.vertical_ui .appr_view_cont .content_table .tb_cont {background: #f9f9f9;}
.vertical_ui .appr_view_cont .content_table .tb_cont td {display:none;}
.vertical_ui .appr_view_cont .content_table .tb_cont.open td {display:table-cell;}
.vertical_ui .appr_view_cont .content_table .flip {padding-right:0; margin:-3px 0 0 0;}
.vertical_ui .appr_view_cont .content_table .txtc {text-align: center}

.appr_view_cont .vertical_table {padding:0;}

/*결재 목록뷰 테이블 스타일 180306*/
.container_area.approval {overflow-y: hidden;}
.container_area.approval .content_table.title_mg0 .title { margin:0; max-width: 100%; padding-left:16px; padding-right: 24px; }
.container_area.approval .content_table.title_mg0.return .title { padding-right: 42px; }
/*미리보기 아이콘 버튼*/
.container_area.approval .content_table.title_mg0 .title .ico_preview { position: absolute; right: 0; top: -2px; display: inline-block; width: 20px; height: 19px; margin-right: 0; font-size: 0; background: url(../img/ico_appr_lst.png) 0 0 no-repeat; }
.container_area.approval .content_table.title_mg0 .title .ico_preview:hover { background: url(../img/ico_appr_lst.png) 0 -19px no-repeat; }

.container_area.approval .content_table.title_mg0.return .title .ico_preview { right: 20px; }
.container_area.approval .content_table.title_mg0.return .title .ico_return { position: absolute; right: 0; top: -2px; display: inline-block; width: 20px; height: 19px; margin-right: 0; font-size: 0; background: url(../img/ico_appr_lst.png) -20px 0 no-repeat; }
.container_area.approval .content_table.title_mg0.return .title .ico_return:hover { background: url(../img/ico_appr_lst.png) -20px -19px no-repeat; }

.appr_view_cont .content_table .table_head th .form {vertical-align: top; /*ie*/}

.vertical_ui .appr_view_cont .paginate_area { border-top: none; }

/* 결재 라인 */
.appr_line_list {}
.appr_line_list ul {margin:-5px 0 0 0;}
.appr_line_list ul li {display:block;}
.appr_line_list ul li:after {clear:both;}
.appr_line_wrap {position: relative; min-height: 53px; padding:0 0 20px 0; background: url('../img/bg_appr_dot.png') 10px 0 repeat-y;}
.appr_line_wrap .appr_line_progress {position: absolute; left:0; top:0; width:66px;}
.appr_line_list .appr_line_progress .ic {position: absolute; left:0; top:0; display:block; width: 20px; height: 20px; text-indent: -9999px; background: url('../img/ic_appr_01.png') left top no-repeat;}
.appr_line_list .appr_line_progress .red_dot {position: absolute; left:14px; top:0; display:block; width: 4px; height: 4px; text-indent: -9999px; background:#ff5959; border:2px solid #fff; border-radius: 50%; box-sizing: content-box;}
.appr_line_list .appr_line_progress .txt {position: absolute; left:25px; top:-11px; display:table; width:70px; height:40px; color:#797f9c;}
.appr_line_list .appr_line_progress .txt span {display:table-cell; vertical-align: middle;}
.appr_line_list .appr_line_man {position: relative; margin:0 200px 0 100px; padding:0 0 0 43px; min-height:27px;}

.appr_line_list .appr_line_man .photo {position: absolute; left:0; top:-5px; overflow: hidden; display:block; width:32px; height:32px; vertical-align: top; font-size:0; text-align:center; background:url('../img/profile_photo_blank.png') no-repeat left top; border-radius:50%;}
.appr_line_list .appr_line_man .photo img {width:100%; height:100%;}
.appr_line_list .appr_line_man .name {display:inline-block; color:#111;}
.appr_line_list .appr_line_man .name em {font-weight: bold;}
.appr_line_list .appr_line_man .btn_msg {display:inline-block; vertical-align: middle; width: 18px; height: 18px; text-indent: -9999px; background:url('../img/btn_appr_msg.png') 3px 5px no-repeat;}
.appr_line_list .appr_line_state {position: absolute; right:6px; top:1px;}
.appr_line_list .appr_line_state .state { padding: 0 8px; }
.appr_line_list .appr_line_date {position: absolute; right:88px; top:3px; font-size: 12px; color:#969696; text-align: right;}
.appr_line_list .appr_line_msg {position: relative; display:none; padding: 15px; margin:14px 0 5px 100px; color:#333; border:1px solid #d9d9d9; border-radius: 2px;}
.appr_line_list .appr_line_msg:after {content:''; position: absolute; left:-1px; top:-10px; display:block; width: 11px; height: 10px; background:url('../img/bg_appr_arrow.png') left top no-repeat;}
.appr_line_list .appr_line_msg.open {display:block;}
.appr_line_list ul li:last-child .appr_line_wrap {min-height:auto; padding:0; background: none;}
.appr_line_wrap.ing {background: url('../img/bg_appr_line.png') 10px 0 repeat-y;} /* 대기중 */
.appr_line_wrap.ing .appr_line_progress .ic {background-position: 0 -30px;}/* 대기중 */
.appr_line_wrap.ing .appr_line_progress .txt {color:#3858ed;}/* 대기중 */
.appr_line_wrap.end {background: url('../img/bg_appr_line.png') 10px 0 repeat-y;} /* 완료 */
.appr_line_wrap.end .appr_line_progress .ic {background-position: -30px 0;}/* 완료 */
.appr_line_wrap.end .appr_line_progress .txt {color:#3858ed;}/* 완료 */

.arrp_block_wrap .inner.appr_line_list  {position: relative; width:100%; padding:0}
.arrp_block_wrap .arrp_block_cont .sche_share_list.appr_box {max-height: 350px; margin:0;}
.inner.appr_line_list ul {margin: 0}
.inner.appr_line_list  > ul > li {position: relative; width:100%; display:block; min-height:60px; border-top:1px solid #eee; }
.inner.appr_line_list li:first-child {border-top:0 none}
.inner.appr_line_list  > ul > li > div {position: relative; padding: 10px 20px 10px 0;}
.inner.appr_line_list .appr_line_usr {position: relative; margin: 0 66px 0 0; padding: 0 0 0 40px;  min-height: 27px;}
.inner.appr_line_list .appr_line_usr .team { font-size: 12px; white-space: nowrap; overflow: hidden; text-overflow: ellipsis}
.inner.appr_line_list .appr_line_usr.no_name .team { margin-top:10px;}
.inner.appr_line_list .appr_line_usr .name { display: inline-block; color: #969696; font-size: 12px;}
.inner.appr_line_list .appr_line_usr .name em {color: #111}
.inner.appr_line_list .appr_line_state {top:9px; right:20px;}
.inner.appr_line_list .ico_re {position:absolute; top: 13px; right: 70px; width:12px; height:12px; display: block; background: url('../img/ico_appr_re.png') 0 0 no-repeat;}
.inner.appr_line_list .appr_line_date {top:29px; right:20px;}
.inner.appr_line_list .appr_line_usr .btn_msg { display: inline-block; vertical-align: middle; width: 18px; height: 18px; text-indent: -9999px; background: url(../img/btn_appr_msg.png) 1px 3px no-repeat; }

.appr_line_num {position: absolute; display: table-cell; vertical-align: middle;    
    left: 0; top: 0; width: 40px; height: 60px; text-align: center; line-height: 60px; color: #797f9c;}
.inner.appr_line_list .appr_line_msg {  padding: 15px; margin: 14px 0 5px 40px; color: #333; border: 1px solid #d9d9d9; border-radius: 2px;}

/* 스퀘어에서 사용되는 '일정'을 위해 관련 스타일 common으로 추가 */
.square .btns_area .ico_btn.my_reservation { width: inherit; font-size: 12px !important; padding-left: 28px; padding-right: 10px; background: url('../img/bg_reservation.png') no-repeat 0 0; margin-right: 8px; }
.square .btns_area  .ico_btn.my_reservation.on { background-position: 0 -28px; border: 1px solid #c2c2c2; }
.square .btns_area  .ico_btn.my_reservation:hover { text-decoration: none; background-color: #f5f5f5; border: 1px solid #c2c2c2; }
.square .btns_area  .ico_btn.my_reservation:active { color: #333; background-color: #fff; border: 1px solid #969696; }

.square .btns_area  .ico_btn.flag { width: inherit; font-size: 12px !important; padding-left: 30px; padding-right: 10px; background: url('../img/bg_form.png') no-repeat 10px -234px; margin-right: 8px; }
.square .btns_area  .ico_btn.flag.on { background-position: 10px -274px; border: 1px solid #c2c2c2; }
.square .btns_area .ico_btn.flag:hover { text-decoration: none; background-color: #f5f5f5; border: 1px solid #c2c2c2; }
.square .btns_area  .ico_btn.flag:active { color: #333; background-color: #fff; border: 1px solid #969696; }

/*조직도 자동완성 180307*/
.token-input-dropdown.auto_frame { max-height: 280px; border: 1px solid #999; border-radius: 2px; }
.token-input-dropdown.auto_frame ul li { padding: 5px 10px 8px; color: #666; cursor: pointer; }
.token-input-dropdown.auto_frame .frame_area { display: table; min-height: 36px; }
.token-input-dropdown.auto_frame .frame_area .photo, .token-input-dropdown.auto_frame .frame_area .info { display: table-cell; vertical-align: middle; }
.token-input-dropdown.auto_frame .frame_area .photo { padding-right: 8px; }
.token-input-dropdown.auto_frame .frame_area .photo img { width: 26px; height: 26px; border-radius: 100px; }
.token-input-dropdown.auto_frame .frame_area .info .name { color: #111; font-weight: bold; }
.token-input-dropdown.auto_frame .frame_area .info .team, .token-input-dropdown.auto_frame .frame_area .info .mail { color: #666666; }
.token-input-dropdown.auto_frame .frame_area .info .team:before { content: ''; display: inline-block; width: 2px; height: 16px; margin-right: 4px; background: url('../img/dott_gray.png') left 50% no-repeat }
.token-input-dropdown.auto_frame .frame_area .info .mail { display: block; }


