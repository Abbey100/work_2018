.pick_cal input[type="checkbox"] + label .color {position:absolute; top:4px; left:0; display:block; width:12px; height:12px; min-height: 12px; vertical-align:top; border:1px solid #c2c2c2; background-color: #fff; border-radius: 2px; padding: 0 !important; }
.form_table .pick_cal input[type="checkbox"] + label .color {top:9px; min-height: auto;}
