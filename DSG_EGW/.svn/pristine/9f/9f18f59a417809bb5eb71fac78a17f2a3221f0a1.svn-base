td.TopLeft { width: 3px; height: 8px; font-size: 7px; text-align: left; background: white url(/img/TBL_TOP_BG.GIF) repeat-x; }
td.TopMdl { width: 100%; height: 8px; font-size: 7px; background: white url(/img/TBL_TOP_BG.GIF) repeat-x; }
td.TopRght { width: 2px; height: 8px; font-size: 7px; text-align: right; background: white url(/img/TBL_TOP_BG.GIF) repeat-x; }


/* 8.3 추가 스타일 */

/*** 기본 버튼 영역 Button Area ***/
.btn_area { position:relative; padding:10px 18px; clear:both; background:#f3f3f3; border-bottom:1px solid #dadada; min-height:29px }
    .btn_area .btns { position:relative;margin:-15px 0 0 0; height: 29px;padding:0;}
    .btn_area .btns li { list-style: none; display: inline-block;}
        .btn_area .btns span { display:inline-block; height:29px; padding-left:1px; background:url(/img/btn_white.png) no-repeat left 0; vertical-align:top;  }
        .btn_area .btns span:hover { background-position:left -29px; }
            .btn_area .btns span a { display:block; height:29px; padding-right:10px; padding-left:9px; background:url(/img/btn_white.png) no-repeat right 0; text-align:center; color:#333; line-height:30px; font-size: 12px; text-decoration: none;}
            .btn_area .btns a:hover { background-position:right -29px; text-decoration:none; position:relative; z-index:1000; border-right:1px }
            .btn_area .btns a:active { background-position:right -58px; }
            .btn_area .btns a.active {background-position:right -58px }
            .btn_area .btns a.active span { background-position:left -58px }
                      .btn_area .btns.first { padding-right:5px; margin-right:10px; background:url(/img/line_btn_area_top_btns.gif) no-repeat right 3px }



.lst_move {vertical-align: middle;
    width: 28px;
    margin: 0 42px 0 4px;
    float: left;}
.lst_move a {
    float: left;
}
.lst_move .add {
    float: left;
    text-indent: -999px;
    width: 28px;
    height: 26px;
    display: inline-block;
    background: url(/img/btn_lst_slct_move.png) no-repeat 0 -25px;
    
}
.lst_move .del {
    float: left;
    text-indent: -999px;
    width: 28px;
    height: 26px;
    display: inline-block;
    background: url(/img/btn_lst_slct_move.png) no-repeat 0 0;margin-top: -1px;
}

/** 기본 버튼 **/

/* left Menu */
.Submenu {border-right:1px solid #bfbfbf;}
.Submenu table.left tr td {padding:0 12px; box-sizing: border-box;}
.Submenu table.left tr td a {color:#666; font-size: 12px; text-decoration: none; vertical-align: text-top;}
.Submenu table.left tr td a:hover {color: #F26522;}
.Submenu table.left tr:first-child td {padding-top:12px;}

/* contents_area */
.contents_area {margin:0; padding:0; }
.contents_area .title_area {width:100%; background: #f3f3f3; position: relative; border-bottom: 1px solid #dadada;}
.contents_area .contents_box {width:100%; padding:0; clear: both;}

.contents_box tr td table:hover { }

.inner_box {display: block; padding: 0 20px; position: relative; width:100%; box-sizing: border-box;}
.inner_box td.TtlOnTop {padding:18px 0 10px !important;}
.inner_box th {border-left:0 none;}
.inner_box tr {padding: 4px 6px;}

.btn_smll {font-size:12px;padding:6px 0;}
.btn_smll span {display: inline-block; height: 21px; padding-left: 1px; background: url(/img/btn_white_xs.gif) no-repeat left 0; }
.btn_smll span a {display: block; height: 21px; padding-right: 6px; padding-left: 5px; background:url(/img/btn_white_xs.gif) no-repeat right 0;line-height: 22px;color: #555;font-size: 11px;text-decoration: none;}

