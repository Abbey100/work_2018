@charset "utf-8"; 

.container_area.square .container {padding: 0 40px;}
.container_area.square .scroll_area {border-top: 1px solid #969696; }
/*.container_area.square .square_wrap.with_btn { height: calc(100% - 91px); }*/

.square_area {position:relative; height: 100%; padding-top:15px;padding-bottom:50px;/*padding-right:15px;*/}

.square_area .section { width: calc(100% - 235px); height: 100%; padding-right:35px;padding-bottom:50px;}
.square_area .aside { position: absolute; top: 15px; right: 0; width:235px; height: 100%; padding-bottom:35px;}

.no_srched {text-align: center; height: 320px; padding-top: 160px; background:url(../img/no_srch.png) no-repeat center 88px; }
.no_srched p {font-weight: bold;}

.no_cont {text-align: center; height: 320px; padding-top: 100px;  }
.no_cont p {font-weight: bold;}

.feedWrite {position:relative; display:block; width:100%; height:60px;  padding:0 20px; margin-bottom:15px; text-align:right; color:#969696;  background:#f5f5f5;}
.feedWrite p {float:left; line-height:58px; font-size:15px;}
.feedWrite p:first-child {width: calc(100% - 170px); text-align: left;}
.feedWrite p:nth-child(2) {float:right;}
.feedWrite p span {display:inline-block; position:relative; width:20px; height:20px;  font-size:0; vertical-align:middle;  background:url(../img/square/ico.png) no-repeat left top;}
.feedWrite p span:hover {background:url(../img/square/ico_over.png) no-repeat left top;}
.feedWrite p span::before {content:''; position:absolute; left:-9px; top:-25px; display:none; width:39px; height:23px; color:#fff; font-size:11px; line-height:18px; text-align:center; background:url(../img/square/bg_tooltip.png) no-repeat left top;}
.feedWrite p span:hover::before {display:block;}
.feedWrite p span.photo::before {content:'사진';}
.feedWrite p span.file {background-position:left -40px; margin-left:10px;}
.feedWrite p span.file::before {content:'파일';}
.feedWrite p span.plan {background-position:left -80px; margin-left:9px;}
.feedWrite p span.plan::before {content:'일정';}
.feedWrite p span.report {background-position:left -120px;  margin-left:12px;}
.feedWrite p span.report::before {content:'보고';}
.feedWrite p span.noti {background-position:left -160px;  margin-left:12px;}
.feedWrite p span.noti::before {content:'공지';}

.feedsNotice {margin-top:22px;margin-bottom:15px;border-bottom:1px solid #ccc;}
.feedsNotice > .notice {padding-bottom:6px;}
.feedsNotice > .notice h3 {float:left; padding:0 0 14px 0; }
.feedsNotice > .notice h3 a{font-size:15px; font-weight:bold; color:#2d3f93;}
.feedsNotice > .notice .new {padding-left:5px; font-size:14px; font-weight:bold; color:#3858ed;}
.feedsNotice > .notice h3 a:after {content:'';display:inline-block;width:16px; height:16px; margin-top:3px;margin-left:6px;border:1px solid #ccc; border-radius:10px; font-size:0; background:url(../img/square/bu_arrow.png) no-repeat center center; }
.feedsNotice > .notice ul {clear:both; }
.feedsNotice > .notice ul li {height:26px;}
.feedsNotice > .notice ul li .badge {margin-right:5px; vertical-align:top;}
.feedsNotice > .notice ul li a {display:inline-block;max-width:90%; line-height:20px; white-space: nowrap; overflow: hidden; text-overflow:ellipsis; word-break: break-all;}
.feedsNotice > .notice ul li a:hover {text-decoration:underline;font-weight: bold;}
.feedsNotice > .feedCrumbs {border-top:1px dotted #d5d5d5; border-bottom:none;}

.feedsBobbin {/*padding-right:30px*/}
.feedsBobbin .feedCrumbs:nth-child(1){padding-top:9px;}
.feedCrumbs {position:relative; padding:24px 0 10px 55px;border-bottom:1px solid #ccc; }
.feedCrumbs dt .profile_photo {position:absolute; left:5px; top:25px; width:40px; height:40px;}
.feedCrumbs dt .profile_photo img{width:40px; height:40px;border-radius:20px;}
.feedsBobbin .feedCrumbs:nth-child(1) dt .profile_photo {top:10px;}
.feedCrumbs dt * {float:left;}
.feedCrumbs dt {min-height:22px;}
.feedCrumbs dt a.name {font-size:14px; font-weight:bold; color:#111;}
.feedCrumbs dt a.part {padding-left:5px;font-size:14px; font-weight:bold; color:#2d3f93; background:url('../img/divider.png') left 3px no-repeat;}
.feedCrumbs dt a.part::after {content:''; display:inline-block; width:10px; height:10px;margin-top:5px; background:url(../img/square/bu_arrow_blue.png) no-repeat right 50%;}
.feedCrumbs dt span {padding-right:5px; color:#666; font-size: 13px;}
.feedCrumbs dt span::before {content:'ㆍ'; color:#666;}
.feedCrumbs dd.time {clear:both; font-size:12px; color:#969696;}
.feedCrumbs dd.share {position:absolute; top:37px; right:34px;}
.feedCrumbs dd.share > a {display:block; width:20px; height:20px; font-size:0; background:url(../img/square/ico.png) no-repeat left -200px;}
.feedCrumbs dd.tool {position:absolute; top:37px; right:10px; }
.feedCrumbs dd.tool > a {display:block; width:20px; height:20px; font-size:0;  background:url(../img/square/ico.png) no-repeat left -240px;}
.feedCrumbs dd.share.open .select_lst, .feedbox dd.tool.open .select_lst, .feedCrumbs dd.tool.open .select_lst {display:block; position:absolute; right:0; top:20px;}

.feedCrumbs dd.article {padding-top:17px; line-height:22px;}
.feedCrumbs dd.article .txt {max-width:960px; padding-bottom:11px; font-size: 13px;}
.feedCrumbs dd.article .txt a.more {display:block; margin-top:5px; color:#969696;}
.feedCrumbs dd.article .txt a.more:hover {color:#333; text-decoration:underline;}
.feedCrumbs dd.article em, .feedCrumbs dd.article .at, .feedCrumbs dd.article .tag {font-weight:bold; color:#2d3f93; font-size: 12px;}
.feedCrumbs dd.article .no_link em, .feedCrumbs dd.article .no_link .at, .feedCrumbs dd.article .no_link .tag {font-weight:normal; color:#333; font-size: 12px;}
.feedCrumbs dd.article > .tag {padding:5px 0 25px 0; border-top:1px solid #eeeeee;}
.feedCrumbs dd.article > .tag span{padding-right:8px;}
.feedCrumbs .attached .file_info dt:first-child {margin-top: 0;}
.feedCrumbs .attached .btn_open {margin-top: 4px;}
.feedCrumbs .attached .file_info .txt {padding-bottom: 0;}
.feedCrumbs .attached .file_lst.download {margin-top: 0;}
.feedCrumbs .attached .file_lst.download li {padding: 0 68px;}
.feedCrumbs .attached .form {margin-top: 2px;}
.feedCrumbs .attached .ico_btn {margin-top: 2px;}

.feedCrumbs .linkfeed {}
.feedCrumbs .linkfeed > a {display:block; position:relative; min-height:66px;  margin-bottom:10px; font-size:12px; line-height:19px; color:#969696; border:1px solid #e3e3e3;}
.feedCrumbs .linkfeed > a:last-child {margin-bottom:0;}
.feedCrumbs .linkfeed > a em {display:block; font-size:13px; color:#333;padding-right: 10px;}
.feedCrumbs .linkfeed > a:hover em {text-decoration: underline;}

.feedCrumbs .linkfeed.no_link {}
.feedCrumbs .linkfeed.no_link > a {color:#969696; }
.feedCrumbs .linkfeed.no_link > a:last-child {margin-bottom:0;}
.feedCrumbs .linkfeed.no_link > a em {display:block; font-size:13px; color:#333;padding-right: 10px;}
.feedCrumbs .linkfeed.no_link > a:hover em {text-decoration: none;}



.feedCrumbs .linkfeed > a img {position:absolute;}
.feedCrumbs .linkfeed .plan {padding:13px 0 13px 65px; background:url(../img/square/ico_plan.png) no-repeat 24px 17px;}
.feedCrumbs .linkfeed .plan em::after {content:''; display:inline-block; width:13px; height:19px; background:url(../img/square/bu_arrow_dark_gray.png) no-repeat center 5px;}

.feedCrumbs .linkfeed .link {height:70px; padding:6px 0 0 80px; background:url(../img/square/bg_link.png) no-repeat left top;}
.feedCrumbs .linkfeed .link img {left:0; top:0;}
.feedCrumbs .linkfeed .link span {position:absolute; right:9px; bottom:5px;}

.feedCrumbs .linkfeed .member {padding:0 0 0 65px; line-height:62px;}
.feedCrumbs .linkfeed .member img {top:13px; left:13px; width:40px; height:40px; border-radius:20px;}
.feedCrumbs .linkfeed .member em {display:inline; font-size:14px; font-weight:bold; color:#111;}
.feedCrumbs .linkfeed .member span {font-size:13px; color:#333;}
.feedCrumbs .linkfeed .member span::before {content:'ㆍ'; color:#afafaf;}
.feedCrumbs dd.article > .linkfeed + .tag {border:none;}

.feedCrumbs dd.article .attached {}
.feedCrumbs .linkfeed + .thum_area, .feedCrumbs .linkfeed + .attached,  .feedCrumbs .thum_area + .attached {padding-top:10px;}

.feedCrumbs dd.article .txt:last-child {padding-bottom:25px;}
.feedCrumbs dd.article:last-child .txt {padding-bottom:11px;}
.feedCrumbs dd.article .linkfeed:last-child, .feedCrumbs dd.article .attached:last-child {padding-bottom:30px;}
.feedCrumbs dd.article:last-child .linkfeed:last-child {padding-bottom:15px;}

.feedCrumbs dd.feedback {box-sizing:content-box; height:20px; padding:0 0 10px 3px;}
.feedCrumbs dd.feedback > * {float:left;}
.feedCrumbs dd.feedback .flag {margin:2px 13px 0 0;}
.feedCrumbs dd.feedback .face {position:relative;}
.feedCrumbs dd.feedback .face a.set {display:inline-block; height:16px; width:16px; margin-top:2px; background:url(../img/square/ico_face.png) no-repeat left top;}
.feedsBobbin.visitor .feedCrumbs dd.feedback .face a.set {background-position:left -31px}
.feedCrumbs dd.feedback .face a.score {padding:0 5px 0 0; font-size:0;}
.feedCrumbs dd.feedback .face a.score img {position:relative; z-index:3; }
.feedCrumbs dd.feedback .face a.score img:nth-child(2) {z-index:2; left:-6px;}
.feedCrumbs dd.feedback .face a.score img:nth-child(3) {z-index:1; left:-12px;}
.feedCrumbs dd.feedback .face a.score span {position:relative; left:-9px; font-size:13px; color:#333; vertical-align:top;}
.feedCrumbs dd.feedback .face a.score:hover span {text-decoration: underline;}
.feedsBobbin.visitor .feedCrumbs dd.feedback .face a.score span {color:#969696;}
.feedsBobbin.visitor .feedCrumbs dd.feedback .face a.score:hover span {text-decoration:none;}
.feedCrumbs dd.feedback .face ul {display:none; position:absolute; top:-45px; left:-31px; width:192px; height:42px; padding:6px 0 0 6px; background:url(../img/square/bg_face_select.png) no-repeat left top;}
.feedCrumbs dd.feedback .face.open ul {display:block;}
.feedCrumbs dd.feedback .face ul li {float:left;  padding:0 0 0 3px;}
.feedCrumbs dd.feedback .face ul li input {position:absolute; opacity:0;}
.feedCrumbs dd.feedback .face ul li label {display:block; width:27px; height:24px; font-size:0; background:url(../img/square/ico_face_selcet.png) no-repeat left -24px; }
.feedCrumbs dd.feedback .face ul li:nth-child(1) label {background-position:left -24px; }
.feedCrumbs dd.feedback .face ul li:nth-child(1) input:checked + label {background-position:left 0; }
.feedCrumbs dd.feedback .face ul li:nth-child(2) label {background-position:-30px -24px; }
.feedCrumbs dd.feedback .face ul li:nth-child(2) input:checked + label {background-position:-30px 0; }
.feedCrumbs dd.feedback .face ul li:nth-child(3) label {background-position:-60px -24px; }
.feedCrumbs dd.feedback .face ul li:nth-child(3) input:checked + label {background-position:-60px 0; }
.feedCrumbs dd.feedback .face ul li:nth-child(4) label {background-position:-90px -24px; }
.feedCrumbs dd.feedback .face ul li:nth-child(4) input:checked + label {background-position:-90px 0; }
.feedCrumbs dd.feedback .face ul li:nth-child(5) label {background-position:-120px -24px; }
.feedCrumbs dd.feedback .face ul li:nth-child(5) input:checked + label {background-position:-120px 0; }
.feedCrumbs dd.feedback .face ul li:nth-child(6) label {background-position:-150px -24px; }
.feedCrumbs dd.feedback .face ul li:nth-child(6) input:checked + label {background-position:-150px 0; }

.feedCrumbs dd.feedback .replylen {height:16px; line-height:14px; padding-left:23px; margin-top:3px; color:#969696; background:url(../img/square/ico_reply.png) no-repeat left 1px; cursor: pointer; font-size:12px;}
.feedsBobbin.visitor .feedCrumbs dd.feedback .replylen {background-position:left -31px}

.thum_area {position:relative; }
.thum_wrap {position:relative; left:1px; min-width:410px; max-width:1109px; height:136px; overflow: hidden; }
.thumnails {position:relative; width:2000px;}
.thumnails .thumnail {display:inline-block; margin-right: 4px;}
.thumnails .thumnail span {display: inline-block; width:136px; height:136px; overflow: hidden; cursor: pointer;}
.thumnails .thumnail span img {object-fit: cover; object-position:center; transition: -webkit-transform .2s cubic-bezier(.165,.84,.44,1); transition: transform .2s cubic-bezier(.165,.84,.44,1); transition: transform .2s cubic-bezier(.165,.84,.44,1),-webkit-transform .2s cubic-bezier(.165,.84,.44,1); }
.thumnails .thumnail:hover span img { transition-duration: .3s; -webkit-transform: scale(1.05); -ms-transform: scale(1.05); transform: scale(1.05); }
.thum_wrap .btn {position:absolute; width:30px; height:136px; margin:0; display: block; background: url(../img/square/btn_thumnails.png) 0 0 no-repeat; z-index: 2; font-size: 0; cursor: pointer;}
.thum_wrap .btn.pre {top:0; left:0; background-position: 0 0;}
.thum_wrap .btn.next {top:0; right:0; background-position: -30px 0;}
.thum_wrap:hover .btn {display: block;}

.feedCrumbs .replybox{margin:0; border-top:0 none; padding:0; }
.feedCrumbs .replybox .write {position:relative; padding:11px 0 11px 20px; background:#f5f5f5;}
.feedCrumbs .replybox .write .textComplete{min-height:18px; max-height:initial;margin-right:140px;border-bottom: 0 none;color:#969696;outline: none;}
.feedCrumbs .replybox .write .textComplete p{line-height: 22px}
.feedCrumbs .replybox .write > p {position:absolute; right:10px; top:10px;}
.feedCrumbs .replybox .write > p a {float:left; background:url(../img/square/ico2.png) no-repeat left top;}
.feedCrumbs .replybox .write > p a.photo { width:16px; height:16px; margin-left:10px;margin-top:2px; font-size:0;}
.feedCrumbs .replybox .write > p a.file { width:16px; height:16px; background-position:left -32px; margin-left:10px; margin-top:2px;font-size:0;}
.feedCrumbs .replybox .write > p a.submit {position:relative; padding:0 10px 0 11px; background:none; color:#969696;margin-left:7px;}
.feedCrumbs .replybox .write > p a.submit::before {position:absolute; left:0; top:4px; display:inline-block; width:1px; height:12px;  content:''; background:#d9d9d9;}
.feedCrumbs .replybox .write .selectedMention, .feedCrumbs .replybox .write .selectedTag{border:0 none;font-weight:bold;color:#2d3f93;}

.feedCrumbs .replybox .list {padding:30px 0 0 0; }
.feedCrumbs .replybox .list li {position:relative; padding:0 0 26px 45px; min-height:34px;}
.feedCrumbs .replybox .list li .profile_photo{position:absolute; left:0; top:0; width:34px; height:34px}
.feedCrumbs .replybox .list li .profile_photo img{width:34px; height:34px;border-radius:17px;}
.feedCrumbs .replybox .list li .name {font-weight:bold; color:#111;}
.feedCrumbs .replybox .list li .name:hover, .feedCrumbs .replybox .list li div .at:hover {text-decoration: underline;}
.feedCrumbs .replybox .list li .time {font-size:12px; color:#969696;}
.feedCrumbs .replybox .list li div {padding-top:5px; font-size:12px;}
.feedCrumbs .replybox .list li div .at {color:#2d3f93;}
.feedCrumbs .replybox .list li .btn_modify{position: absolute;top:-2px;right:19px;}
.feedCrumbs .replybox .list li .btn_modify a{display:inline-block;width:17px;height:17px;background: url(../img/square/ico_modify.png) 0 0 no-repeat;font-size:0;vertical-align:top;}
.feedCrumbs .replybox .list li .btn_modify a.del{background-position: 0 0;margin-left:9px;}
.feedCrumbs .replybox .list li .btn_modify a.modify{background-position: 0 -33px;}
.feedCrumbs .replybox .list li .btn_modify a:nth-child(2):before{content:'';display:inline-block;width:1px;height:12px;margin-top:3px;margin-left:-8px;background-color: #d9d9d9;}

.feedCrumbs .replybox .list li .file{font-size: 13px;}
.feedCrumbs .replybox .list li .file:before{content:'';display:inline-block;width:16px;height:18px;background: url(../img/common.png) -104px -48px no-repeat;}
.feedCrumbs .replybox .list li .file a{color:#333;font-weight: bold;}
.feedCrumbs .replybox .list li .file a:hover{text-decoration: underline;}
.feedCrumbs .replybox .list li .file span{padding-left:4px;color:#969696;font-weight: bold;}

.feedCrumbs .replybox .list li .reply_modify{position:relative;margin-top:5px;border:1px solid #e3e3e3;padding:11px 96px 11px 19px;}
.feedCrumbs .replybox .list li .reply_modify .textComplete{max-width:920px;min-height:18px; /*max-height:66px;*/padding:0;border:0 none;font-size:12px;color:#333;}
.feedCrumbs .replybox .list li .reply_modify .textComplete p{padding:0;}
.feedCrumbs .replybox .list li .reply_modify .selectedMention, .feedCrumbs .replybox .list li .reply_modify .selectedTag{border:0 none;font-weight:bold;color:#2d3f93;}
.feedCrumbs .replybox .list li .reply_modify > p {position:absolute; right:20px; top:8px;}
.feedCrumbs .replybox .list li .reply_modify .btn_save{color:#3958ed;}
.feedCrumbs .replybox .list li .reply_modify .btn_save:after{content:'';display:inline-block;width:1px;height:12px;margin:3px 6px 0 10px;background-color: #d9d9d9;}
.feedCrumbs .replybox .list li .reply_modify .btn_cancel{color:#969696;}

.feedCrumbs .replybox:last-child {padding-bottom:15px;}

.feedCrumbs .replybox .write.focus {background:#fff; border:1px solid #e3e3e3;}
.feedCrumbs .replybox .write.focus > p a {background:url(../img/square/ico2_over.png) no-repeat left top;}
.feedCrumbs .replybox .write.focus > p a.photo { width:16px; height:16px; margin-left:10px; font-size:0;}
.feedCrumbs .replybox .write.focus > p a.file { background-position:left -32px;}
.feedCrumbs .replybox .write.focus > p a.submit {color:#3958ed;background: none;}

.feedCrumbs a.btn_feed_open, .feedCrumbs a.btn_feed_close{display:block;color:#111;text-align:center;font-size:12px;}
.feedCrumbs a.btn_feed_open:after{content:'';display:inline-block;width:9px;height:10px;margin-top:6px;margin-left:8px;background: url(../img/square/btn_feed_arrow.png) 0 -10px no-repeat;}
.feedCrumbs a.btn_feed_close:after{content:'';display:inline-block;width:9px;height:10px;margin-top:6px;margin-left:8px;background: url(../img/square/btn_feed_arrow.png) 0 0px no-repeat;}
.feedCrumbs a.btn_feed_open:hover, .feedCrumbs a.btn_feed_close:hover {text-decoration:underline;}

.linkfeed .btn_modify{position: absolute;top:50%;right:19px;margin-top: -9px;}
.linkfeed .btn_modify a{display:inline-block;width:17px;height:17px;background: url(../img/square/ico_modify.png) 0 0 no-repeat;font-size:0;vertical-align:top;}
.linkfeed .btn_modify a.del{background-position: 0 0;margin-left:9px;}
.linkfeed .btn_modify a.modify{background-position: 0 -33px;}
.linkfeed .btn_modify a:nth-child(2):before{content:'';display:inline-block;width:1px;height:12px;margin-top:3px;margin-left:-8px;background-color: #d9d9d9;}


/* 워크피드 우측 영역 */
.aside .invite{padding:19px 20px 23px;margin-bottom:15px;background-color: #c6d6f7;font-size:12px;color:#333;}
.aside .invite .time{display:block;color:#969696;margin-top:3px;}
.aside .invite .btn_sec{margin-top:10px;}
.aside .invite .btn_sec .btn{border:0 none;margin-right:0px;margin-left:-4px;}
.aside .group_info{position:relative;margin-bottom:15px;background-color: #ebf1fc;}
.group_info .group_img{}
.group_info .group_img img{width:100%;max-width: 236px;}
.group_info_con{padding:14px 25px 20px;}
.group_info_con .title{padding-bottom:2px;line-height:17px;font-size:14px;font-weight:bold;color:#111;}
.group_info_con span{font-size:12px;color:#666;}
.group_info_con span.date:after{content:'';display:inline-block;width:1px;height:12px;margin:4px 5px 0 6px;background-color: #d6dde7;}
.group_info_con .txt{padding-top:3px;line-height:19px;color:#333;}
.group_info .group_notice{position:absolute;top:-3px;right:11px;}
.group_info .group_notice .btn_notice, .group_info .group_notice .btn_set{display:inline-block;margin-right:-6px;}
.group_info .group_notice .btn_notice a.ico, .group_info .group_notice .btn_set a.ico{display:inline-block;width:28px;height:28px;border:1px solid #c2c2c2;border-radius:2px 2px;font-size:0;}
.group_info .group_notice .btn_notice.open a.ico, .group_info .group_notice .btn_notice a.ico:hover, 
.group_info .group_notice .btn_set.open a.ico, .group_info .group_notice .btn_set a.ico:hover{border:1px solid #969696;}
.group_info .group_notice .btn_notice a.ico{background: #fff url(../img/square/ico.png) 3px -277px no-repeat;}
.group_info .group_notice .btn_set a.ico{background: #fff url(../img/square/ico.png) 3px -317px no-repeat;}
.group_info .group_notice .btn_set .select_lst{display:none;right:-2px;}
.group_info .group_notice .btn_notice .notice_box{display: none}
.group_info .group_notice .btn_notice.open .notice_box, .group_info .group_notice .btn_set.open .select_lst{display: block;z-index: 100;}
.group_info .group_notice .select_lst li a{font-size:12px;}
.group_info .group_notice .btn_notice.open .notice_box{position:absolute;top:43px;right:28px;width:183px;padding:9px 0 12px;font-size:12px;background-color: #fff;border:1px solid #969696;border-radius:2px 2px;}
.group_info .group_notice .btn_notice.open .notice_box .notice_tab{padding:0 9px;}
.group_info .group_notice .btn_notice.open .notice_box .notice_tab a{display:inline-block;width:50%;margin-right:-4px;height:26px;line-height:23px;border:1px solid #969696;text-align:center;}
.group_info .group_notice .btn_notice.open .notice_box .notice_tab a:first-child{border-radius:2px 0 0 2px;}
