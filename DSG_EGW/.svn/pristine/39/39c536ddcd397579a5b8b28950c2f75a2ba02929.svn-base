<!doctype html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<meta charset="utf-8" />
<title>HANDY GROUPWARE</title>
<link rel="stylesheet" href="../../css/common.css" type="text/css" />
<link rel="stylesheet" href="../../css/jquery-ui-1.10.4.custom.min.css" type="text/css" />
<link rel="stylesheet" href="/javascripts/jquery.fileUpload/fileUpload.css" type="text/css" />
<!--[D] jqueryUI -->
<script type="text/javascript" src="../../js/jquery-latest.min.js"></script>
<script type="text/javascript" src="../../js/common.js"></script>
</head>
<body>
<div class="pop_wrap">
  <div class="container_box meeting">
      
    <!-- 버튼 영역 -->
    <div class="title_area">
      <h1>업무그룹 등록</h1>
    </div>
    <div class="btn_area">
      <ul class="btns">
        <li><span><a href="#">추가</a></span></li>
        <li><span><a href="#">닫기</a></span></li>
      </ul>
    </div>
    <!-- //버튼 영역 -->
    <!-- 글쓰기 -->
<div class="view_content_box">
    <div class="inner_box">
    <div class="view_body_contents writting" style="height: 637px;">
    <div class="pop_container">
        <div class="write_box">
            <table class="meeting_table" width="100%">
                <colgroup>
                    <col style="width:110px;">
                    <col style="width:*">
                    <col style="width:110px;">
                    <col style="width:*">
                </colgroup>
                <tbody>
                    <tr class="subject">
                    <th><span class="tit">제목<span class="essntal">*</span></span></th>
                    <td class="title" colspan="3">
                        <input type="text" class="basic_inp" style="IME-MODE:active;WIDTH:100%;" tabindex="1" maxlength="50"></td>
                    </tr>
                    
                    <tr>
                    <th><span class="tit">시작일</span></th>
                    <td><div class="wr_form_td"> 
                      <div class="datepic_box">
                        <input type="text" class="basic_inp" value="2014.03.10" style="width:100px;" readonly>
                        <a href="#"><img src="../../img/btn_datepicker.gif" alt="달력" class="btn_date"></a> </div>
                      <select class="basic_select">
                        <option value="12">12</option>
                      </select>
                      <span class="pd_rgt_7_lft_1">시</span>
                      <select class="basic_select">
                        <option value="31">31</option>
                      </select>
                      <span class="pd_rgt_7_lft_1">분</span> </div></td>
                    <th><span class="tit">종료일</span></th>
                    <td><div class="wr_form_td"> 
                      <div class="datepic_box">
                        <input type="text" class="basic_inp" value="2014.03.10" style="width:100px;" readonly>
                        <a href="#"><img src="../../img/btn_datepicker.gif" alt="달력" class="btn_date"></a> </div>
                      <select class="basic_select">
                        <option value="12">12</option>
                      </select>
                      <span class="pd_rgt_7_lft_1">시</span>
                      <select class="basic_select">
                        <option value="31">31</option>
                      </select>
                      <span class="pd_rgt_7_lft_1">분</span> </div></td>
                    </tr>
                    
                    <tr>
                        <th>공개/보안여부</th>
                        <td><div class="wr_form_td">
                          <label class="pd_rgt_20">
                            <input type="radio">
                            공개</label>
                          <label class="pd_rgt_20">
                            <input type="radio">
                            비공개</label>
                        </div></td>
						</tr>
                    
                    <tr class="incharge">
                        <th><span>관리자<span class="essntal">*</span></span></th>
                        <td class="name">
                            <span class="btn_human">관리자</span>
                            <input type="text" class="basic_inp" tabindex="2" style="IME-MODE:active;WIDTH:60%;" id="to" name="to">
                            <label class="pd_rgt_20"><input type="checkbox">주재자와 동일</label>
                        </td>
                        <th><span>주재자<span class="essntal">*</span></span></th>
                        <td class="name">
                            <span class="btn_human">주재자</span>
                            <input type="text" class="basic_inp" tabindex="2" style="IME-MODE:active;WIDTH:100%;" id="to" name="to">
                        </td>
                    </tr>
                    
                    <tr class="incharge">
                        <th><span>참여자<span class="essntal">*</span></span></th>
                        <td class="name" colspan="3">
                            <span class="btn_human">참여자</span>
                            <input type="text" class="basic_inp" tabindex="2" style="IME-MODE:active;WIDTH:100%;" id="to" name="to">
                        </td>
                    </tr>
                    
                    <tr class="incharge">
                        <th><span>열람자</span></th>
                        <td class="name" colspan="3">
                            <span class="btn_human">열람자</span>
                            <input type="text" class="basic_inp" tabindex="2" style="IME-MODE:active;WIDTH:100%;" id="to" name="to">
                        </td>
                    </tr>
                    <tr class="subject">
                    <th><span class="tit">설명<span class="essntal">*</span></span></th>
                    <td colspan="3">
                    <div class="wr_form_td">
                        <textarea class="basic_txtarea" style="width:100%; height: 60px;" wrap="hard"></textarea>
                    </div>
                    </tr>
                </tbody>
            </table>
      
        </div>

    </div>
    </div>
    </div>
    
  <!-- // 글쓰기 --> 
  </div>
</div>
</div>
</body>
</html>
