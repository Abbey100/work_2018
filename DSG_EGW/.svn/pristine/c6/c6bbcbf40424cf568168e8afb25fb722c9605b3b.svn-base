<html lang="ko">
<head>
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<meta charset="utf-8" />
<title>HANDY Groupware 8.3</title>
<link rel="stylesheet" href="../css/login.css" type="text/css" />
<script type="text/javascript" src="../js/placeholder.js"></script>
</head>
<body>
<!-- login works client -->
<div class="works_login">
  <h2 class="logo"></h2>
  <div class="login">
    <fieldset>
      <legend>그룹웨어 로그인</legend>
      <!-- 로그인 인풋영역 start -->
      <ul style="display:block">
        <li>
          <label for="user_id"><span><img src="../img/bg_input.png"></span> <span class="user_img"><img src="../img/login_email.png"></span></label>
          <div>
            <input type="text" id="user_id" title="이메일주소를 입력해주세요" placeholder="example@handysoft.co.kr">
          </div>
        </li>
        <li>
          <label for="user_pwd"> <span><img src="../img/bg_input.png"></span> <span class="user_img"><img src="../img/login_pwd_works.png"></span></label>
          <div>
            <input type="password" id="user_pwd" title="비밀번호를 입력해주세요" placeholder="비밀번호">
          </div>
        </li>
      </ul>
      <!-- 로그인버튼 start -->
      <div class="btn_login"> <a href="#">로그인</a></div>
      <!-- // 로그인버튼 end -->
    </fieldset>
  </div>
</div>
</body>
</html>
