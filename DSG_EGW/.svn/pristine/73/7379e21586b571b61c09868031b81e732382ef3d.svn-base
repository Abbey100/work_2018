@charset "UTF-8";

/* 서브 타이틀 */
.sub_title{border-bottom:1px solid #202020;height:31px;color:#202020;margin-bottom:14px;}
.sub_title span{font-size:16px;letter-spacing: -0.7px;}
.sub_title span.title{display:inline-block;max-width:100%;line-height:18px;white-space:nowrap;overflow:hidden;text-overflow: ellipsis;word-break:break-all;}
.sub_title.view_gap{margin-bottom:37px;}
.sub_title.slogan_title{margin-bottom:30px;}

.sub_title_02{position:relative;height:24px;color:#333;}
.sub_title_02 span.title{display:inline-block;max-width:100%;height:23px;line-height:18px;padding-right:5px;font-size:14px;letter-spacing: -0.7px;white-space:nowrap;overflow:hidden;text-overflow: ellipsis;word-break:break-all;}
.sub_title_02 span.title.board_open, .sub_title_02 span.title.board_close{cursor: pointer;}
.sub_title_02 span.title.board_open:after{content:'';display:inline-block;width:10px;height:6px;margin-left:13px;background: url('../img/sp_common.png') no-repeat -194px -50px;vertical-align:1px;}
.sub_title_02 span.title.board_close:after{content:'';display:inline-block;width:10px;height:6px;margin-left:13px;background: url('../img/sp_common.png') no-repeat -212px -50px;vertical-align:2px}
.sub_title_02 span.title.line{border-bottom:1px solid #202020;}

/* paginate */
.paginate_area{position:absolute;bottom:0;width:calc(100% - 215px);height:58px;padding:14px 0;text-align:center;border-top:1px solid #e3e3e3; box-sizing: border-box;background-color:#fff;z-index:999;}
.paginate_area .paginate_box{display:inline-block;margin:0 auto;}
.popup_layer .paginate_area{width:calc(100% - 60px);}
.paginate_area a{width:25px;height:22px;line-height:22px;float:left;border:1px solid #ddd;border-left:0 none;font-size:12px;color:#999;}
.paginate_area a:first-child{border-left:1px solid #ddd}
.paginate_area a:hover{background-color:#f7f7f7}
.paginate_area a.btn_first, .paginate_area a.btn_pre, .paginate_area a.btn_next, .paginate_area a.btn_last{font-size:0;}
.paginate_area a.btn_first:before{content:'';display:inline-block;width:9px;height:7px;background: url('../img/sp_common.png') no-repeat -72px -50px;vertical-align:-4px}
.paginate_area a.btn_pre:before{content:'';display:inline-block;width:4px;height:7px;background: url('../img/sp_common.png') no-repeat -91px -50px;vertical-align:-4px}
.paginate_area a.btn_last:before{content:'';display:inline-block;width:9px;height:7px;background: url('../img/sp_common.png') no-repeat -119px -50px;vertical-align:-4px}
.paginate_area a.btn_next:before{content:'';display:inline-block;width:4px;height:7px;background: url('../img/sp_common.png') no-repeat -105px -50px;vertical-align:-4px}
.paginate_area a.current{margin-left:-1px;border:1px solid #606060;background-color:#606060;color:#fff;}
.paginate_area .board_num{position:absolute;top:10px;right:2px;width:73px;text-align:left;}
/*.paginate_area .board_num .select_group ul{max-height:90px;margin-top:26px;}
.paginate_area .board_num .select_group .passiveSelect, .paginate_area .board_num .select_group .activeSelect{height:26px;line-height:25px;}*/
.content_section.tree_out .paginate_area{width:calc(100% - 0px);}

/* 게시판 목록 상단 버튼 */
.list_btn_area{position:relative;float:left;height:38px;margin:20px 0 0;}
.left_btn{float:left;padding-right:11px;}
.left_btn a{display:inline-block;height:26px;line-height:24px;margin-right:1px;padding:0 10px;border:1px solid #b4b4b4;border-radius:2px 2px;background-color:#fff;color:#111;font-size:12px;text-align:center;vertical-align: middle;}
.left_btn a:hover{background-color:#f6f6f6;}
.left_btn a:active{border:1px solid #808080;background-color:#fff;}
.left_btn a.btn_save{border:1px solid #606060;background-color:#606060;color:#fff;}
.left_btn a.btn_save:hover{border:1px solid #999;background-color:#999;}
.left_btn a.btn_save:active{border:1px solid #333;background-color:#333;}
.left_btn .txt {display:inline-block; height:28px; line-height:28px; font-size:14px; font-weight:bold; color:#333;}
.left_btn .btn_date_down{display:inline-block;}
.left_btn .btn_date_down:before{content:'';display:inline-block;width:1px;height:11px;margin:0 6px 0 5px;background-color:#ccc;vertical-align:-3px}
.left_btn .text_update{display:inline-block;vertical-align:-2px;}
.left_btn .text_update:before{content:'';display:inline-block;width:1px;height:11px;margin:0 8px 0 5px;background-color:#ccc;vertical-align:-2px;}
.left_btn .select_group .passiveSelect, .left_btn .select_group .activeSelect{height:26px;line-height:24px;}
.left_btn .select_group ul{margin-top:26px;}

/* 우측 버튼 */
.util_right.right_btn {margin-top:-32px;}
.right_btn{float:right;padding-right:11px;}
.right_btn a{display:inline-block;height:26px;line-height:24px;margin-right:1px;padding:0 10px;border:1px solid #b4b4b4;border-radius:2px 2px;background-color:#fff;color:#111;font-size:12px;text-align:center;vertical-align: middle;}
.right_btn a:hover{background-color:#f6f6f6;}
.right_btn a:active{border:1px solid #808080;background-color:#fff;}
.right_btn a.btn_save{border:1px solid #606060;background-color:#606060;color:#fff;}
.right_btn a.btn_save:hover{border:1px solid #999;background-color:#999;}
.right_btn a.btn_save:active{border:1px solid #333;background-color:#333;}
.right_btn .txt {display:inline-block; height:28px; line-height:28px; font-size:14px; font-weight:bold; color:#333;}
.right_btn .btn_date_down{display:inline-block;}
.right_btn .btn_date_down:before{content:'';display:inline-block;width:1px;height:11px;margin:0 6px 0 5px;background-color:#ccc;vertical-align:-3px}
.right_btn .text_update{display:inline-block;vertical-align:-2px;}
.right_btn .text_update:before{content:'';display:inline-block;width:1px;height:11px;margin:0 8px 0 5px;background-color:#ccc;vertical-align:-2px;}
.right_btn .select_group .passiveSelect, .right_btn .select_group .activeSelect{height:26px;line-height:24px;}
.right_btn .select_group ul{margin-top:26px;}


.list_btn_area.none{float:none;height:28px;margin:0;padding-top:20px;}

.list_filter_area{position:relative;display:inline-block;margin:4px 11px 0 2px;}
.list_filter_area a.btn_list_filter{position:relative;display:block;color:#666;padding-right:13px;}
.list_filter_area a.btn_list_filter:after{content:'';position:absolute;top:50%;right:0;width:7px;height:4px;margin-top:-2px;background: url('../img/sp_common.png') no-repeat -277px -30px;}
.list_filter_area a.btn_list_filter.on:after{background: url('../img/sp_common.png') no-repeat -277px -43px;}
.list_filter_area.only {margin-right:3px;}
.list_filter_area.only a.btn_list_filter:after {background-position: -276px -26px}
.list_filter_area .list_filter_box{position:absolute;left:-2px;margin-top:16px;min-width:93px;border:1px solid #000;background-color:#fff;z-index: 99}
.list_filter_area .list_filter_box ul li a{display:block;height:27px;line-height:26px;color:#666;padding:0 20px 0 26px;white-space:nowrap}
.list_filter_area .list_filter_box ul li a:hover{background-color:#f0fefb;}
.list_filter_area .list_filter_box ul li a.on{background: #f0fefb url('../img/sp_common.png') no-repeat -280px -79px;}

/* 목록 상단 검색 영역 */
.list_btn_area .right_srch{float:right;}
.list_srch_box{display:block;height:28px;padding:0;border:1px solid #c2c2c2;border-radius:2px 2px;background-color:#fff;}
.list_srch_box .selectbox{display: inline-block;min-width: 80px}
.list_srch_box .input_group{display: inline-block;padding:0 36px 0 0;border:0 none;vertical-align: top}
.list_srch_box .input_group input.form_control{width:190px;height:28px;line-height:28px;}
.list_srch_box .input_group a.btn_srch{}
.list_srch_box a.btn_srch_detail{ display: inline-block; height: 28px;line-height: 28px;padding: 0 10px; vertical-align: top; border-left: 1px solid #c2c2c2;}
/* 상세 검색 영역 */
.cont_srch_area {position:absolute; right:0; width:100%; min-height:38px; margin-top:20px;}
.no_tab .cont_srch_area {margin-top:10px;}
.cont_srch_area.open {position:relative; height:auto; padding:38px 0 10px;}
.cont_srch_area .right_srch{position:absolute; right:0; top:0;}
.detail_srch_wrap {display: none; }
.detail_srch_wrap .radio_list li {padding-right:15px;}
.open .detail_srch_wrap {display: block;background: #f7f7f7; padding: 14px;}
.detail_srch {position:relative;width:100%;table-layout: fixed;}
.detail_srch th, .detail_srch td {padding:6px;}
