.detail_srch th .input_group input.form_control, .detail_srch td .input_group input.form_control {height:26px;}
.detail_srch .thead {}
.detail_srch th .thead {padding-left:30px;}
.detail_srch th:first-child .thead {padding-left:0}
.detail_btn_area{float:right;}
a.text_btn{margin-right:10px;text-decoration: underline;line-height:1.8;}
/* 게시판 상세 하단 버튼 */
.view_btn_area {position:relative;height:35px;padding:10px 0;border-top:1px solid #ccc;background:#fff; text-align:center;}
.view_btn_area a{display:inline-block;width:138px;height:33px;line-height:31px;border-radius:2px 2px;color:#fff;font-size:14px;}
.view_btn_area a.btn_complete{margin:0 3px;border:1px solid #202020;background-color:#202020;}
.view_btn_area a.btn_complete:hover{border:1px solid #474747;background-color:#474747;}
.view_btn_area a.btn_complete:active{border:1px solid #000;background-color:#000;}
.view_btn_area a.btn_cancel, .view_btn_area a.btn_del{margin:0 3px;border:1px solid #333;background-color:#fff;color:#333}
.view_btn_area a.btn_cancel:hover, .view_btn_area a.btn_del:hover{background-color:#eee;}
.view_btn_area a.btn_cancel:active, .view_btn_area a.btn_del:active{background-color:#ddd;}
.view_btn_area a.btn_list{position:absolute;left:0;width:68px;height:30px;line-height:30px;border:1px solid #ccc;background-color:#f7f7f7;color:#666;font-size:13px;}
.view_btn_area a.btn_list:before{content:'';display:inline-block;width:6px;height:10px;margin-right:9px;background: url('../img/sp_common.png') no-repeat -1px -50px;vertical-align:0px;}
.view_btn_area a.btn_list:hover{background-color:#eee;}
.view_btn_area a.btn_list:active{background-color:#ddd;}

