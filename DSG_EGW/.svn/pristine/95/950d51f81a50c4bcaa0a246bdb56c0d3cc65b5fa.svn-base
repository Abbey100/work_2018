@charset "utf-8";
/* CSS Document */
.guide_tit { border-bottom: 3px solid #74441b; color: #424242; font-size: 16px; font-weight: bold; margin: 0 auto; padding: 20px }
.guide_tit img { margin-right: 50px; }
.guide_contents { padding:0 0 30px ; position: relative;   } 
.sample {<min-height:15></min-height:15>0px; margin-bottom:30px;}
.guide_h { background: url(../guide/images/bu_tit.gif) no-repeat 5px 7px; border-bottom: 1px solid #c9c9c9; color: #424242; font-size: 30px; line-height:1.5em; font-weight: bold;  margin: 20px 0; padding: 0 0 10px 15px; }
.guide_h  span { display:block; font-size:12px; line-height:18px }
.location h3 { font-size:16px; color:#fff; font-weight:bold  }
#snb h2 { font-size:16px; color:#fff; font-weight:bold }

#header ul { position:fixed; left:0; top:0; width:100%; height:30px; overflow:hidden; background:#666; z-index:100000}
#header ul li { float:left; width:20%; text-align:center; line-height:30px; height:30px; font-size:15px; color:#fff; font-weight:bold; }
#header ul li a { color:#fff }
.guide_box { padding-top:40px; }