/* 그리드형 */
.grid_list_head {padding: 2px 17px 2px 17px; border-top: 1px solid #969696;border-bottom: 1px solid #eee; background: #f3f6fb;}
.grid_list_head .sorting {font-size:12px;color:#4c4c4c;}
.grid_list_head .sorting span {line-height: 24px;}
.grid_list_head .sorting .selectbox {display: inline-block; width:auto; vertical-align: top;}
.grid_list_head .sorting .selectbox .ui-button {height: auto; border: none; padding: 0 5px;}
.grid_list_head .sorting .selectbox .ui-selectmenu-text {margin-right: 15px;padding: 0}
.grid_list_head .sorting .selectbox .ui-menu .ui-menu-item-wrapper {padding: 5px 8px 7px 8px;}
.grid_list {margin: 0; padding-top: 30px;}
.grid_list:after {content: ''; display: block; clear: both;}
.grid_list li {float: left; width: 178px; height: 242px; padding: 0 5px;}

.grid_list li dl {position: relative; width: 178px; padding: 0 5px;}
.grid_list li dl:hover dd.file {background: #fafafa;}
.grid_list li.selected dd.file,
.grid_list li.selected dl:hover dd.file {border: 1px solid #3858ed; background: #edf3ff;}
.grid_list li dt {position: absolute;	left: 0; top: 168px;width: 100%;margin: 8px 0;text-align: center;}
.grid_list li dt .filename {display: inline-block; width: 168px; height: 40px; line-height: 19px;color: #333;overflow:hidden;-webkit-line-clamp: 2; text-overflow: ellipsis;-webkit-box-orient: vertical; word-wrap:break-word;}
.grid_list li dd .form {margin-left: 0;margin-right: 1px;}
.grid_list li dd.file {width: 168px; height: 168px;	border: 1px solid #e3e3e3;}
.grid_list li dd.file a {display: inline-block; width: 166px; height: 136px; margin-top: 30px;	}
.grid_list li dd.file.parent_folder {background: none;}
.grid_list li dd.file img {width: 166px; height: 136px;}
.grid_list li dd.file .item	{position: absolute; top: 63px; left: 66px; width: 46px; height: 46px; background: url('../img/ico_file_large.png') no-repeat 0px 0px; font-size: 0; line-height: 0;}
.grid_list li dd.file.folder .item {background-position: 0 4px;}
.grid_list li dd.file.parent_folder .item {background-position: 0 -44px;}
.grid_list li dd.file.shared_folder .item {background-position: 0 -92px;}
.grid_list li dd.file.fellow_folder .item {background-position: 0 -140px;}
.grid_list li dd.file.zip .item {background-position: 2px -190px;}
.grid_list li dd.file.pdf .item {background-position: 2px -242px;}
.grid_list li dd.file.xls .item {background-position: 2px -294px;}
.grid_list li dd.file.doc .item {background-position: 2px -346px;}
.grid_list li dd.file.ppt .item {background-position: 2px -398px;}
.grid_list li dd.file.txt .item {background-position: 2px -452px;}
.grid_list li dd.file.hwp .item {background-position: 2px -504px;}
.grid_list li dd.file.etc .item {background-position: 2px -556px;}
.grid_list li dd.file.video .item {background-position: 2px -652px;}
.grid_list li dd.file.music .item {background-position: 2px -710px;}
.grid_list li dd.file.no_image .item {background-position: 2px -614px;}
.grid_list li dd.check {position: absolute; top: 7px; left: 16px;}



/* 파일 확장자 아이콘 */
.ico_file {display:inline-block; width:16px; height:16px; background: url(../img/ico_file.png) 0 0 no-repeat; font-size: 0; vertical-align: top;}
.ico_file.doc{background-position: 0 0}
.ico_file.xls{background-position: 0 -22px}
.ico_file.ppt{background-position: 0 -44px}
.ico_file.zip{background-position: 0 -66px}
.ico_file.pdf{background-position: 0 -88px}
.ico_file.folder{background-position: 0 -109px}
.ico_file.shar{background-position: 0 -129px}
.ico_file.txt{background-position: 0 -150px}
.ico_file.hwp{background-position: 0 -172px}
.ico_file.etc{background-position: 0 -194px}
.ico_file.image{background-position: 0 -216px}
.ico_file.video{background-position: 0 -238px}
.ico_file.music{background-position: 0 -260px}


