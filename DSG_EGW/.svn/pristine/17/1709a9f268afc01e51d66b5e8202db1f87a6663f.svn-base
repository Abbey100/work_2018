.notice_area {
	padding: 40px 50px;
	position:absolute;
	bottom:0;
	height:282px;
	display: block;
	width:100%; 
	background: #0b2c82; /* Old browsers */
	background: -moz-linear-gradient(left, #0b2c82 0%, #0a4997 70%, #094191 100%); /* FF3.6-15 */
