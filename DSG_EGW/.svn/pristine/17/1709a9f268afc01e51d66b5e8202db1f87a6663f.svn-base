@charset "utf-8"; 
@import url('common.css');

/** layout **/
.wrapper { position: relative; width: 100%; height: 100%; }

/* header */
.header { position: fixed; left:0; z-index: 100; width:100%; min-width:800px; height:59px; background:#fff; border-bottom: 1px solid #bfbfbf; }
.header h1 {display:inline-block; padding:0 0 0 88px; line-height:58px; font-size: 19px; color: #111; font-weight: normal; letter-spacing: -1px;} 
.header h1 img { vertical-align: middle; } 
.header .aside {position:absolute; top:13px; right:30px;}
.header fieldset {float:left; width:290px; height:32px; font-size:0; border:1px solid #d9d9d9; border-radius: 50px}
.header fieldset .selectbox {display:inline-block; width:88px; vertical-align:top;}
.header fieldset .selectbox span {border:none; line-height:30px; width:auto;}
.header fieldset .ui-button {height: 30px; margin-left: 5px;}
.header fieldset input {width:125px; height:30px; line-height:30px; border: 0 none; outline: 0 none;}
.header fieldset .btn_srch {display:inline-block; width:26px; height:30px; vertical-align:top; background: url('../img/ico_zoom.png') no-repeat 4px 50%;  font-size: 0;}
.header fieldset .btn_srch_more {display: inline-block; width:46px; height:18px; line-height:16px; margin-top:5px; vertical-align:top; text-align:center; font-size: 12px; color:#4c4c4c; border-left:1px solid #d9d9d9;}
.header .util {float:left; padding-left:12px; font-size:0;}
.header .util a {display:inline-block; width:34px; height:34px; margin-left:12px; vertical-align: top; font-size:0; text-align:center; background:url(../img/ico_util.png) no-repeat left top;}
.header .util a.btn_admin {background-position:left top;}
.header .util a.btn_me {background:none;}
.header .util a.usr_more {width:15px; background: url('../img/ico_up_down.png') right 16px no-repeat; padding:0; margin: 0;}
.header .util a.usr_more.open {background-position:right -30px;}
.header .util a.btn_me img {width:26px; height:26px; margin-top:3px; border-radius:50%;}
.header .util a.btn_noti {position:relative; background-position:left -60px;}
.header .util a.btn_noti span {display:none;}
.header .util a.btn_noti span.on {position:absolute; right:5px; top:7px; display:inline-block; width:8px; height:8px; background:#ff5959; border-radius:50%; border:2px solid #fff;}
.header .util a.btn_support {background-position:left -120px;}
.header .tab {display: inline-block; height:21px;}
.header .ui-button:hover {background-color: transparent}

/* gnb */
.gnb_area { position: fixed; left:0; top: 0; display: block; width:58px; height: 100%; 
	background:-moz-linear-gradient(top, #1c1c8c 0%, #13a1f2 100%); /* FF3.6-15 */
	background:-webkit-linear-gradient(top, #1c1c8c 0%,#13a1f2 100%); /* Chrome10-25,Safari5.1-6 */
	background:linear-gradient(to bottom, #1c1c8c 0%,#13a1f2 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter:progid:DXImageTransform.Microsoft.gradient( startColorstr='#1c1c8c', endColorstr='#13a1f2',GradientType=0 ); /* IE6-9 */z-index: 100; }

.gnb_menu {position:relative; width: 100%; height:calc(100% - 96px); overflow: hidden;margin-top:58px;}

/* home btn */
.home_area{position: fixed; top:0; width:58px;height: 58px; border-bottom: 1px solid rgba(255, 255, 255, 0.15); z-index: 9999;}
.home_area a { display: block; width: 100%; height: 100%; font-size: 0;}
.home_area a i{display:inline-block;font-size:0;padding: 0; margin: 0;position: absolute;left:0}
.home_area a i.icon{width: 58px;height: 58px;background-image: url('../img/gnb.png');}
.home_area a i.home {background-position: 0 0;font-size: 0}
.home_area a span{color: #fff;font-size:14px;display: inline-block;text-align: left; position: absolute;text-overflow: ellipsis; white-space: nowrap;overflow: hidden;height: 58px;line-height: 58px;padding-left: 58px;}

/* 트랜지션 */
.gnb_area:hover, .gnb_area:hover .home_area, .gnb_area:hover .gnb_menu, .gnb_area:hover .gnb_menu li, .gnb_area:hover .controll_area
{	width: 150px;
    transition: all .25s cubic-bezier(.25, .46, .45, .94);
	-webkit-transition: all .25s cubic-bezier(.25, .46, .45, .94);
	-moz-transition: all .25s cubic-bezier(.25, .46, .45, .94);
	-ms-transition: all .25s cubic-bezier(.25, .46, .45, .94);
	-o-transition: all .25s cubic-bezier(.25, .46, .45, .94);
	/*
	-webkit-transition: all all .25s cubic-bezier(.25, .46, .45, .94) ease-out;
	-moz-transition: all all .25s cubic-bezier(.25, .46, .45, .94) ease-out;
	-o-transition: all all .25s cubic-bezier(.25, .46, .45, .94) ease-out;
	transition: all all .25s cubic-bezier(.25, .46, .45, .94) ease-out;
	*/}

.gnb_area.open, .gnb_area.open .home_area, .gnb_area.open .gnb_menu, .gnb_area.open .gnb_menu li, .gnb_area.open .controll_area {width: 150px;}
.gnb_menu .menus {position:absolute; top:0; display: block; height: 100%; }

.gnb_menu li { position:relative; display:block; height:58px; width:58px;}
.gnb_menu li.selected, .gnb_menu li:hover, .home_area.selected, .home_area:hover {background-color: rgba(0,0,0,0.3);}

.gnb_menu li a { display:block; width: 100%; width: 100%;}
.gnb_menu li a i{display:inline-block;font-size:0;padding: 0; margin: 0;position: absolute;left:0}
.gnb_menu li a i.icon{width: 58px;height: 58px;background-image: url('../img/gnb.png');}
.gnb_menu li a i.mail {background-position: -58px 0;} 
.gnb_menu li a i.board { background-position: -116px 0; } 
.gnb_menu li a i.worknet { background-position: -174px 0; } 
.gnb_menu li a i.square { background-position: -232px 0; } 
.gnb_menu li a i.approval { background-position: -290px 0; } 
.gnb_menu li a i.schedule { background-position: -348px 0; } 
.gnb_menu li a i.equipment { background-position: -406px 0; } 
.gnb_menu li a i.storage { background-position: -464px 0; } 
.gnb_menu li a i.contact { background-position: -522px 0; } 
.gnb_menu li a i.community { background-position: -580px 0; } 
.gnb_menu li a i.research { background-position: -638px 0; } 
.gnb_menu li a i.linked { background-position: -696px 0; } 
.gnb_menu li a i.app_doc { background-position: -754px 0; } 
.gnb_menu li .menu_name {display: inline-block; color: #fff; font-size:14px; text-overflow: ellipsis; white-space: nowrap; overflow: hidden; height: 58px; line-height: 58px; padding-left: 58px;}
.gnb_menu li .dot_new {position:absolute; display: none; top:15px; left:39px; width:4px; height:4px; background: #ff5959; border-radius: 4px; font-size: 0;}
.gnb_menu li.new .dot_new {display: block;}

.controll_area{position: fixed; bottom:0; width: 58px;}
.controller{position: relative; display: block; width:100%;  height: 36px; border-top: 1px solid rgba(255, 255, 255, 0.3); font-size: 0; z-index: 101;}

.gnb_area .controller a {position:absolute; top:0; display: block; width:29px; height: 36px;}
.gnb_area:hover .controller a, .gnb_area.open .controller a {width:50%;}
.gnb_area .controller a.down_btn {left:0; background: url('../img/down_btn.png') 12px 10px no-repeat;}
.gnb_area .controller a.up_btn {left: 50%; background:  url('../img/up_btn.png') 0 10px no-repeat;}
.gnb_area:hover .controller a.down_btn, .gnb_area.open .controller a.down_btn{ background-position-x: 29px }
.gnb_area:hover .controller a.up_btn, .gnb_area.open .controller a.up_btn { background-position-x: 20px;}


/* 링크 SNS 팝업 */
.link_wrap{position: fixed; left:130px; z-index:999;display:none;}
.link_wrap.open {display:block;}
.link_wrap .link {width:193px; z-index:999999;background: #fff; box-sizing:border-box; border:1px solid #999; border-radius: 2px;}

.link_wrap.top {top:0;}
.link_wrap.bottom {bottom:0;}
.link_wrap .link .arrow {position:absolute; display:block; width: 7px; height: 11px; margin:0 0 0 -7px; background:url('../img/ly_box01_left.png') no-repeat left top; font-size: 0;}
.top .arrow{ top:22px;}
.bottom .arrow{ top:100px;}


.link .sns_head {height: 25px;}
.link .sns_head .sns_head_close {position: absolute; right:10px; top:11px; display:block; width: 11px; height: 11px; text-indent: -9999px; background:url('../img/btn_sns_close.png') no-repeat 0 0;}
.link .sns_cont_wrap {min-height:180px; padding:0 0 10px 0;}
.sns_cont {max-height:180px; overflow: auto}
.sns_cont .sns_cont_list li {display:block; padding:0 9px; margin:0 0 14px 0; color:#969696;}
.sns_cont .sns_cont_list li:last-child {margin:0;}
.sns_cont .sns_cont_list .txt_wrap i{display:inline-block;font-size:0;position: absolute;}
.sns_cont .sns_cont_list .txt_wrap i.icon{width: 16px;height: 16px;background-image: url('../img/ico_sns.png');}
.sns_cont .sns_cont_list .txt_wrap i.facebook{background-position: 0 0;}
.sns_cont .sns_cont_list .txt_wrap i.naver{background-position:  0 -16px;}
.sns_cont .sns_cont_list .txt_wrap i.twitter{background-position: 0 -32px;}
.sns_cont .sns_cont_list .txt_wrap i.instagram{background-position: 0 -48px ;} 
.sns_cont .sns_cont_list .txt_wrap i.google{background-position: 0 -64px ;} 
.sns_cont .sns_cont_list .txt_wrap i.default{background-position: 0 -80px ;} 
.sns_cont .sns_cont_list .txt_wrap {display:block; position: relative;  color:#333; font-size: 12px;padding-left:13px; }
.sns_cont .sns_cont_list .txt_wrap .sns_name{display: block; padding-left: 27px;}



/* Contents wrap = body */
.body { position: relative; top: 59px; left: 58px; width: calc(100vw - 58px); height: calc(100vh - 59px); padding: 0; margin: 0; overflow: hidden; }

/* lnb */
.body .lnb_area { position: fixed; top: 58px; left: 58px; display: block; width: 222px; height: calc(100% - 58px); padding: 0 2px 0 0; z-index: 11; border-right: 1px solid #e3e3e3 }
/* Action Button */
.big_action { position: relative; width: 240px; height: 86px; padding:25px 25px 0 25px; font-size: 0;}
.big_action a {vertical-align:top;}
.big_action .action_btn { display: inline-block; width: 172px; height: 40px; text-align: center; line-height: 38px; padding: 0 10px; border-radius: 2px; font-size: 14px; margin-right: 0; }
.big_action.group .action_btn { width: 150px; border-radius: 2px 0 0 2px; }
.big_action .big_split { display: inline-block; width: 22px; height: 40px; background: #314dcc url('../img/btn_action_additional.png') 2px 4px no-repeat; font-size: 0; border-radius:  0 2px 2px 0;}
.big_action .big_split:hover { background-color: #3654d9; }
.big_action .big_split:active { background-color: #203ec7; }
.big_action .select_lst {top: 63px; left:25px; width:172px; background: #5a79ec; border: 0 none; padding: 4px 0; border-radius:  0 0 2px 2px;}
.big_action .select_lst li {padding: 0;}
.big_action .select_lst > li:hover {background: none}
.big_action .select_lst > li a {display: block;}
.big_action .select_lst > li a:hover {background: #223fbf; text-decoration: none;}
.big_action .select_lst > li a:active  {background: #203ec7}
.big_action .select_lst li a { height: 30px; line-height: 28px; padding: 0 15px; color:#fff; font-size: 13px; text-align: left;}
.big_action.half .action_btn {width:86px; height: 40px;}
.big_action.half .action_btn:first-child {border-radius: 2px 0 0 2px; border-right: 1px solid #2343d6;}
.big_action.half .action_btn:last-child {border-radius: 0 2px 2px 0; border-left: 1px solid #4a67ef;}
.big_action.half .action_btn:last-child:active { border-left: 1px solid #223fbf;}


.lnb_lst { position: relative; width: 100%; height: 100%; }
.lnb_menu_area { position: relative; height: calc(100% - 55px); padding: 0 0 0 25px; }
/*.lnb_menu_area .scroll-content { overflow-y: hidden !important; }*/
.big_action + .lnb_menu_area { height: calc(100% - 140px); }
.big_action + .lnb_menu_area.full_height { height: calc(100% - 86px); }
/*.lnb_menu_area .handy-scroll { height: 100% !important; } 20180416 ie의 스크롤 영역 계산이 다르게 적용되기 대문에 해당 스타일 삭제 */
.lnb_menu_area > div { position: relative; height: 100%; width:100%; }
.lnb_menu { position: relative; display: block; padding: 8px 0; margin-right: 18px;}
ul.lnb_menu { padding-top: 0;}
ul.lnb_menu.type2 { padding: 0 8px;}
.lnb_menu a { vertical-align: top; letter-spacing: -0.5px; }
.divide { border-bottom: 1px solid #e3e3e3; }
.dott { border-bottom: 1px dotted #e3e3e3;}

.lnb_menu > li { position: relative; display: block; min-height: 28px; line-height: 28px; padding-left: 12px; }
.lnb_menu > li.dott { border-bottom: 1px dotted #e3e3e3; min-height: 1px !important; line-height:1px; }
/*.lnb_menu_area div.lnb_menu:first-child { padding: 5px 0; }*/
div.lnb_menu { padding: 6px 0; }
ul.lnb_menu.divide { padding-bottom: 12px;}
.divide + .lnb_menu { margin-top: 11px; }
.lnb_menu .menu_title { padding-left: 12px; }
.lnb_menu li a:first-child { display: inline-block; color: #111; font-size: 13px; overflow: hidden; max-width: calc(100% - 20px); text-overflow: ellipsis; white-space: nowrap; }
.lnb_menu li .group a:first-child {padding-left:27px; max-width: calc(100% - 32px); }
.lnb_menu li a:first-child:hover { text-decoration: underline; }
.lnb_menu .selected { color: #3858ed !important; font-weight: bold; }
.lnb_menu .selected+.count { font-weight: bold; }
.lnb_menu li .small { float: right; margin-top: 3px; margin-right: 0; }
.lnb_menu .ico_btn { float: right; margin-right: 0; }
.lnb_menu .menu_title { color: #1d2752; font-size: 13px; font-weight: bold; }
.lnb_menu .menu_title.category { margin-left: -11px; }
.lnb_menu .count { color: #3858ed; font-size: 13px; margin-left:8px;}
.lnb_menu .new { display:inline-block; width:13px;  height:13px; background: url('../img/lnb_new.png') 0 0 no-repeat; font-size: 0; margin: 9px 6px; vertical-align: top;}
.lnb_menu .menu_title.category:before { content: ''; left: 0; display: inline-block; width: 9px; height: 17px; background: url('../img/lnb_depth_menu.png') no-repeat 0 -17px; margin-right: 3px; }
.lnb_menu > .lnb_menu .menu_title.category:before { background: url('../img/lnb_depth_menu_gr.png') no-repeat 0 -17px; }
.lnb_menu.open .menu_title.category:before { background-position: 0 0; }

.lnb_menu.open .inp_group {position:relative; display: block; width:161px; height:28px; margin:5px 0; padding-left:22px; margin-left:13px;}
.lnb_menu.open .inp_group .ico_btn {width: 17px; height: 17px; }
.lnb_menu.open .inp_group .ico_btn.align_lf {position:absolute; left:0; top:6px;}
.lnb_menu.open .inp_group .align_rg {float: none;}
.lnb_menu.open .inp_group .basic_inp {width:100%; height:28px; border:1px solid #d9d9d9; padding: 0 6px; }


.lnb_menu .ico.trash {float:right;}
.lnb_menu li .ico.trash {margin: 6px 0 0;}
.sub_lst { display: none; padding: 5px 0 0 0;}
.open .sub_lst { display: block;}
.sub_lst > li { line-height: 22px; padding: 0 0 0 20px;}
.sub_lst li.selected { line-height: 22px; }
.sub_lst li.selected a { color: #3858ed !important; }
.sub_lst li.sub_title { padding-left:13px; }
.sub_lst li.sub_title:before { content: ''; left: 0; display: inline-block; width: 9px; height: 17px; background: url('../img/bg_flip_open.png') no-repeat 0 8px; }
.sub_lst li.sub_title.open:before {background: url('../img/bg_flip_close.png') no-repeat 0 8px; }
.sub_lst li.sub_title > .sub_lst {display: none; padding: 0 0 5px 0;}
.sub_lst li.sub_title > .sub_lst > li {padding: 0 0 0 15px;}
.sub_lst li.sub_title.open > .sub_lst {display: block; }
.lnb_menu > div { display: none; }
div.lnb_menu.tree { padding:8px 0 0 0; }
div.lnb_menu.divide { padding-bottom: 15px;}
div.lnb_menu.tree > div.lnb_menu.tree { padding-left:8px; margin-right:0;}
.lnb_menu.open > div { display: block;}
.tree .group {font-size: 0; margin-bottom: 6px;}
.tree .group .text_btn { color: #4c4c4c; font-size: 11px; border-right:1px solid #e3e3e3; padding: 0 6px;}
.tree .group .text_btn:last-child {border-right: 0 none;}
.lnb_menu .group {position:absolute; top:8px; right:0;}
.lnb_menu .folder .group {position:relative; top:0;}
.lnb_menu .group .ico_btn {float:left}
.lnb_menu .group .ico_btn:first-child {border-radius: 2px 0 0 2px}
.lnb_menu .group .ico_btn.setting {border-left-width: 0; width:16px; border-radius: 0 2px 2px 0;}
.lnb_menu .group .ico_btn.setting:active {border-left-width: 1px; width:17px; margin-left: -1px;}

/* lnb search */
.lnb_menu .srch_group {position:relative; padding: 16px 0;}
.lnb_menu .srch_group input[type='text'] {width:172px; height: 28px; border: 1px solid #d9d9d9; padding:0 28px 0 10px;}
.lnb_menu .btn_srch {position:absolute; top: 16px; right:0; width: 28px; height:28px; vertical-align:top; background: url('../img/ico_zoom.png') no-repeat 2px 50%;  font-size: 0;}
.lnb_menu .btn_return {position:absolute; top: 16px; right:0; width: 26px; height:28px; vertical-align:top; background: url('../img/btn_search_return.png') no-repeat 50% 50%;  font-size: 0; display:none;}
.lnb_menu .btn_return:before { content:''; width:1px; height:100%; display:inline-block;  background: url('/img/divider.png') no-repeat left 50%;}
.srch_group.on input {padding-right: 48px;}
.srch_group.on .btn_return {display: block;}
.srch_group.on .btn_srch {right: 20px}
/* 동호회 정보 */
.lnb_area .club_info { display: block; padding: 10px 12px 16px 12px; margin-right: 20px;}
.club_name { height: 34px; line-height: 34px; font-weight: bold;}
.club_name.best { background: url('../img/best_club.png') 0 50% no-repeat; padding-left: 23px;}
.lnb_area .club_info .name {display: inline-block; width: calc(100% - 70px); white-space: nowrap; overflow: hidden; text-overflow: ellipsis; line-height: 26px; font-weight: bold;}
.lnb_area .club_info .btn.small {float:right; margin-right: 0;}
.club_info dl { font-size: 0; }
.club_info dl dt, .club_info dl dd {  display: inline-block; font-size: 13px;  line-height: 22px;}
.club_info dl dt { color:#666; width: 70px; }
.club_info dl dd { color:#111;}
.club_info dl dd { color:#111;}
.club_contents .srch_group {position:relative; padding: 16px 0;}
.club_contents .srch_group input[type='text'] {width:179px; height: 28px; border: 1px solid #d9d9d9; padding:0 28px 0 10px;}
.club_contents .btn_srch {position:absolute; top: 16px; right:4px; width: 28px; height:28px; vertical-align:top; background: url('../img/ico_zoom.png') no-repeat 2px 50%;  font-size: 0;}
.club_contents .btn_return {position:absolute; top: 16px; right:4px; width: 26px; height:28px; vertical-align:top; background: url('../img/btn_search_return.png') no-repeat 50% 50%;  font-size: 0; display:none;}
.club_contents .btn_return:before { content:''; width:1px; height:100%; display:inline-block;  background: url('../img/divider.png') no-repeat left 50%;}
.club_contents .srch_group.on input {padding-right: 48px;}
.club_contents .srch_group.on .btn_return {display: block;}
.club_contents .srch_group.on .btn_srch {right: 25px}

/* lnb vip */
.my_vip { position: relative; display: block; height: 22px; line-height: 22px; padding-left: 0;  }

.lnb_menu .usr_mask { position: absolute; top: 0; left: 18px; display: block; width: 22px; height: 22px; background: url('../img/lnb_user_mask.png') 0 0 no-repeat; z-index: 2; }
.lnb_menu .usr_photo { position: absolute; top: 1px; width: 18px; height: 18px; border-radius: 0; }
.lnb_menu .usr_photo img {width: 100%; height: 100%; font-size: 0;}
.lnb_menu .usr_name { padding-left: 25px; height: 22px; line-height: 22px; color: #111; font-size: 13px; }
.lnb_menu .usr_name:hover { text-decoration: underline}
.my_vip .usr_name { display:inline-block; max-width:calc(100% - 30px); font-size: 13px;white-space: nowrap; overflow: hidden; text-overflow: ellipsis; }
.my_vip:hover .usr_name { max-width:calc(100% - 46px)}
.lnb_menu .my_vip .count{ margin-left:0; white-space: nowrap; line-height: 1.2}

/* setting area */
.lnb_lst .setting_area { position: absolute; bottom: 0; height: 55px; width:100%; padding: 22px 25px 0 25px; font-size: 0; background: #fff; }
.lnb_lst .btn_setting { display: inline-block; height: 16px; padding: 0 10px 0 20px; background: url('../img/btn_lnb_setting.png') no-repeat 0 2px; font-weight: bold; font-size: 12px;}
.mail_capacity { display: inline-block; height:13px; padding: 0 0 0 10px; background: url('../img/divider.png') no-repeat 0 4px;}
.now_num { color: #3858ed; font-size: 11px; font-weight: bold;}
.total_num { font-size: 11px;}
.total_num:before { content: '/'; padding: 0 5px; }

/* Contents Area */
.container_area { position: relative; width: calc(100% - 222px);  height: 100%; margin-left: 222px; padding-top: 25px !important; overflow-x: auto; }
.container_area.handy-scroll > div { height:100%; }
.container { position: relative; min-width: 1000px;  height:100%; overflow: hidden; padding: 0 40px;}

.hgroup {clear:both; width:100%; height:40px; padding:5px 0 10px 0;}
.hgroup.keyword .ttl {max-width:200px; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; }
.hgroup.keyword dl {position:relative;font-size: 0; width:auto; margin-top:5px;}
.hgroup.keyword dt, .hgroup.keyword dd {display: inline-block; font-size: 12px; vertical-align: bottom;}
.hgroup.keyword dd {position:relative; color: #3858ed;padding: 0 6px 0 4px;}
.hgroup.keyword dd.d_title {max-width: 110px; white-space: nowrap; overflow: hidden; text-overflow: ellipsis;}
.hgroup.keyword dd.d_sender {cursor: default}
